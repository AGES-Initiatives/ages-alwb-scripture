leosVE.Prayer_NynApolyeis.title = "Song of Simeon."
leosVE.Prayer_NynApolyeis.version = bible_en_US_rsv.version.designation
leosVE.Prayer_NynApolyeis.book = bible_en_US_rsv.Luke
leosVE.Prayer_NynApolyeis.chapverse = "(2:29-32)"
leosVE.Prayer_NynApolyeis.text = "Lord, now lettest thou thy servant depart in peace, according to thy word; for mine eyes have seen thy salvation which thou hast prepared in the presence of all peoples, a light for revelation to the Gentiles, and for glory to thy people Israel."
leosVE.Prayer_NynApolyeis1.text = "Lord, now lettest thou thy servant depart in peace, according to thy word; for mine eyes have seen thy salvation which thou hast prepared in the presence of all peoples."
leosVE.Prayer_NynApolyeis2.text = "A light for revelation to the Gentiles, and for glory to thy people Israel."

