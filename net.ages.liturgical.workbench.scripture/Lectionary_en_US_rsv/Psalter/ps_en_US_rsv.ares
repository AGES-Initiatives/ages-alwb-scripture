psa136.v4.text = "How shall we sing the LORD's song in a foreign land?"
psa136.v5.number = "(136:5 / rsv 137:5)"
psa136.v5.text = "If I forget you, O Jerusalem, let my right hand wither!"
psa136.v6.number = "(136:6 / rsv 137:6)"
psa136.v6.text = "Let my tongue cleave to the roof of my mouth, if I do not remember you, if I do not set Jerusalem above my highest joy!"
psa136.v7.number = "(136:7 / rsv 137:7)"
psa136.v7.text = "Remember, O LORD, against the Edomites the day of Jerusalem, how they said, \"Rase it, rase it! Down to its foundations!\""
