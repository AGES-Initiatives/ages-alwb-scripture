psa2.v8.number = "(2:8 / rsv 2:8)"
psa2.v8.text = "Ask of me, and I will make the nations your heritage, and the ends of the earth your possession."
psa2.v9.number = "(2:9 / rsv 2:9)"
psa2.v9.text = "You shall break them with a rod of iron, and dash them in pieces like a potter's vessel.\""
psa2.v10.number = "(2:10 / rsv 2:10)"
