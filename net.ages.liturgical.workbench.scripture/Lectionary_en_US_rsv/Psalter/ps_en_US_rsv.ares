psa90.v13.number = "(90:13 / rsv 91:13)"
psa90.v13.text = "You will tread on the lion and the adder, the young lion and the serpent you will trample under foot."
psa90.v14.number = "(90:14 / rsv 91:14)"
psa90.v14.text = "Because he cleaves to me in love, I will deliver him; I will protect him, because he knows my name."
psa90.v15.number = "(90:15 / rsv 91:15)"
psa90.v15.text = "When he calls to me, I will answer him; I will be with him in trouble, I will rescue him and honor him."
psa90.v16.number = "(90:16 / rsv 91:16)"
psa90.v16.text = "With long life I will satisfy him, and show him my salvation."

