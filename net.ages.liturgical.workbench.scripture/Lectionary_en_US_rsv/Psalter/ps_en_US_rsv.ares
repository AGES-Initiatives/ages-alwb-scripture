bode1.title = "Ode One"
bode1.version = properties_en_US_rsv.version.designation
bode1.book_abbr = bible_en_US_rsv.Exodus.abbr
bode1.heading1 = "The Song of Moses"
bode1.chapverse = "(15:1-19)"
bode1.heading2 = "Then Moses sang this song to the Lord."
bode1.refrain = "To the Lord I will sing, for he has triumphed gloriously."
bode1.v1.number = "(Ode 1:1)"
bode1.v1.text = "I will sing to the Lord, for he has triumphed gloriously; the horse and his rider he has thrown into the sea."
bode1.v2.number = "(Ode 1:2)"
bode1.v2.text = "The Lord is my strength and my song, and he has become my salvation; this is my God, and I will praise him, my father’s God, and I will exalt him."
bode1.v3.number = "(Ode 1:3)"
bode1.v3.text = "The Lord is a man of war; the Lord is his name."
bode1.v4.number = "(Ode 1:4)"
bode1.v4.text = "Pharaoh’s chariots and his host he cast into the sea; and his picked officers are sunk in the Red Sea."
bode1.v5.number = "(Ode 1:5)"
bode1.v5.text = "The floods cover them; they went down into the depths like a stone."
bode1.v6.number = "(Ode 1:6)"
bode1.v6.text = "Thy right hand, O Lord, glorious in power, thy right hand, O Lord, shatters the enemy. In the greatness of thy majesty thou overthrowest thy adversaries;"
bode1.v7.number = "(Ode 1:7)"
bode1.v7.text = "Thou sendest forth thy fury, it consumes them like stubble."
bode1.v8.number = "(Ode 1:8)"
bode1.v8.text = "At the blast of thy nostrils the waters piled up, the floods stood up in a heap; the deeps congealed in the heart of the sea."
bode1.v9.number = "(Ode 1:9)"
bode1.v9.text = "The enemy said, 'I will pursue, I will overtake, I will divide the spoil, my desire shall have its fill of them. I will draw my sword, my hand shall destroy them.'"
bode1.v10.number = "(Ode 1:10)"
bode1.v10.text = "Thou didst blow with thy wind, the sea covered them; they sank as lead in the mighty waters."
bode1.v11.number = "(Ode 1:11)"
bode1.v11.text = "Who is like thee, O Lord, among the gods? Who is like thee, majestic in holiness, terrible in glorious deeds, doing wonders?"
bode1.v12.number = "(Ode 1:12)"
bode1.v12.text = "Thou didst stretch out thy right hand, the earth swallowed them."
bode1.v13.number = "(Ode 1:13)"
bode1.v13.text = "Thou hast led in thy steadfast love the people whom thou hast redeemed, thou hast guided them by thy strength to thy holy abode."
bode1.v14.number = "(Ode 1:14)"
bode1.v14.text = "The peoples have heard, they tremble; pangs have seized on the inhabitants of Philistia."
bode1.v15.number = "(Ode 1:15)"
bode1.v15.text = "Now are the chiefs of Edom dismayed; the leaders of Moab, trembling seizes them; all the inhabitants of Canaan have melted away."
bode1.v16.number = "(Ode 1:16)"
bode1.v16.text = "Terror and dread fall upon them; because of the greatness of thy arm, they are as still as a stone,"
bode1.v17.number = "(Ode 1:17)"
bode1.v17.text = "Till thy people, O Lord, pass by, till the people pass by whom thou hast purchased."
bode1.v18.number = "(Ode 1:18)"
bode1.v18.text = "Thou wilt bring them in, and plant them on thy own mountain, the place, O Lord, which thou hast made for thy abode, the sanctuary, O Lord, which thy hands have established."
bode1.v19.number = "(Ode 1:19)"
bode1.v19.text = "The Lord will reign for ever and ever."
bode1.v20.number = "(Ode 1:20)"
bode1.v20.text = "For when the horses of Pharaoh with his chariots and his horsemen went into the sea, the Lord brought back the waters of the sea upon them; but the people of Israel walked on dry ground in the midst of the sea."

