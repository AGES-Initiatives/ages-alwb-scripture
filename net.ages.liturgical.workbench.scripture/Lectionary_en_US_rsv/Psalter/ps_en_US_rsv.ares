psa140.v2.text = "Let my prayer be counted as incense before thee, and the lifting up of my hands as an evening sacrifice!"
psa140.v3.number = "(140:3 / rsv 141:3)"
psa140.v3.text = "Set a guard over my mouth, O LORD, keep watch over the door of my lips!"
