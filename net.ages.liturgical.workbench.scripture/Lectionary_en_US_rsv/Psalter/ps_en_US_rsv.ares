psa143.v1.text = "Blessed be the LORD, my rock, who trains my hands for war, and my fingers for battle;"
psa143.v2.number = "(143:2 / rsv 144:2)"
psa143.v2.text = "My rock and my fortress, my stronghold and my deliverer, my shield and he in whom I take refuge, who subdues the peoples under him."
psa143.v3.number = "(143:3 / rsv 144:3)"
psa143.v3.text = "O LORD, what is man that thou dost regard him, or the son of man that thou dost think of him?"
psa143.v4.number = "(143:4 / rsv 144:4)"
psa143.v4.text = "Man is like a breath, his days are like a passing shadow."
psa143.v5.number = "(143:5 / rsv 144:5)"
