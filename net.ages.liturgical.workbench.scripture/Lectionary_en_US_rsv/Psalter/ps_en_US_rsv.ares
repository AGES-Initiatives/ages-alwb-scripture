psa90.v3.text = "For he will deliver you from the snare of the fowler and from the deadly pestilence;"
psa90.v4.number = "(90:4 / rsv 91:4)"
psa90.v4.text = "He will cover you with his pinions, and under his wings you will find refuge; his faithfulness is a shield and buckler."
psa90.v5.number = "(90:5 / rsv 91:5)"
