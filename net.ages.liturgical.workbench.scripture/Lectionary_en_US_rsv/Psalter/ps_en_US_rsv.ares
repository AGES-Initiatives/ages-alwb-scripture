psa39.v13.text = "For evils have encompassed me without number; my iniquities have overtaken me, till I cannot see; they are more than the hairs of my head; my heart fails me."
psa39.v14.number = "(39:14 / rsv 40:13)"
psa39.v14.text = "Be pleased, O LORD, to deliver me! O LORD, make haste to help me!"
psa39.v15.number = "(39:15 / rsv 40:14)"
