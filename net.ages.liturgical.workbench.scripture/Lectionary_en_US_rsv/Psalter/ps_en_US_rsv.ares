psa90.v1.text = "He who dwells in the shelter of the Most High, who abides in the shadow of the Almighty,"
psa90.v2.number = "(90:2 / rsv 91:2)"
psa90.v2.text = "Will say to the LORD, \"My refuge and my fortress; my God, in whom I trust.\""
