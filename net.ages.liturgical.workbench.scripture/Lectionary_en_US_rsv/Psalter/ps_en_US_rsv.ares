psa59.v7.number = "(59:7 / rsv 60:5)"
psa59.v7.text = "That thy beloved may be delivered, give victory by thy right hand and answer us!"
psa59.v8.number = "(59:8 / rsv 60:6)"
psa59.v8.text = "God has spoken in his sanctuary: \"With exultation I will divide up Shechem and portion out the Vale of Succoth."
psa59.v9.number = "(59:9 / rsv 60:7)"
psa59.v9.text = "Gilead is mine; Manasseh is mine; Ephraim is my helmet; Judah is my scepter."
psa59.v10.number = "(59:10 / rsv 60:8)"
psa59.v10.text = "Moab is my washbasin; upon Edom I cast my shoe; over Philistia I shout in triumph.\""
psa59.v11.number = "(59:11 / rsv 60:9)"
psa59.v11.text = "Who will bring me to the fortified city? Who will lead me to Edom?"
psa59.v12.number = "(59:12 / rsv 60:10)"
