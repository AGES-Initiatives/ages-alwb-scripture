bode9.title = "Ode Nine"
bode9.version = properties_en_US_rsv.version.designation
bode9_mag.book_abbr = bible_en_US_rsv.Luke.abbr
bode9_mag.heading1 = "Mary's Song of Praise. The Magnificat."
bode9_mag.chapverse = "(1:46-55)"
bode9_mag.heading2 = "The Maiden Mother sings a hymn to her Son and God."
bode9_mag.intro = "Let us magnify in hymns the Theotokos [and Mother of the Light]."
bode9_mag.v1.number = "(Ode 9a:1)"
bode9_mag.v1.text = "My soul magnifies the Lord, and my spirit rejoices in God my Savior."
bode9_mag.v2.number = "(Ode 9a:2)"
bode9_mag.v2.text = "For he has regarded the low estate of his handmaiden. For behold, henceforth all generations will call me blessed."
bode9_mag.v3.number = "(Ode 9a:3)"
bode9_mag.v3.text = "For he who is mighty has done great things for me, and holy is his name. And his mercy is on those who fear him from generation to generation."
bode9_mag.v4.number = "(Ode 9a:4)"
bode9_mag.v4.text = "He has shown strength with his arm, he has scattered the proud in the imagination of their hearts."
bode9_mag.v5.number = "(Ode 9a:5)"
bode9_mag.v5.text = "He has put down the mighty from their thrones, and exalted those of low degree; he has filled the hungry with good things, and the rich he has sent empty away."
bode9_mag.v6.number = "(Ode 9a:6)"
bode9_mag.v6.text = "He has helped his servant Israel, in remembrance of his mercy, as he spoke to our fathers, to Abraham and to his posterity for ever."
bode9.book_abbr = bible_en_US_rsv.Luke.abbr
bode9.heading1 = "Zechariah's Prophecy: The Benedictus."
bode9.chapverse = "(1:68-79)"
bode9.heading2 = "Zechariah blesses the birth of his son."
bode9.refrain = //"Blessed be the Lord God of Israel, for he has visited and redeemed his people."
bode9.v1.number = "(Ode 9b:1)"
bode9.v1.text = "Blessed be the Lord God of Israel, for he has visited and redeemed his people."
bode9.v2.number = "(Ode 9b:2)"
bode9.v2.text = "And has raised up a horn of salvation for us in the house of his servant David."
bode9.v3.number = "(Ode 9b:3)"
bode9.v3.text = "As he spoke by the mouth of his holy prophets from of old."
bode9.v4.number = "(Ode 9b:4)"
bode9.v4.text = "That we should be saved from our enemies, and from the hand of all who hate us."
bode9.v5.number = "(Ode 9b:5)"
bode9.v5.text = "To perform the mercy promised to our fathers, and to remember his holy covenant."
bode9.v6.number = "(Ode 9b:6)"
bode9.v6.text = "The oath which he swore to our father Abraham, to grant us that we, be[ing] delivered from the hand of our enemies."
bode9.v7.number = "(Ode 9b:7)"
bode9.v7.text = "[That we] might serve him without fear, in holiness and righteousness before him all the days of our life."
bode9.v8.number = "(Ode 9b:8)"
bode9.v8.text = "And you, child, will be called the prophet of the Most High; for you will go before the Lord to prepare his ways."
bode9.v9.number = "(Ode 9b:9)"
bode9.v9.text = "To give knowledge of salvation to his people in the forgiveness of their sins, through the tender mercy of our God."
bode9.v10.number = "(Ode 9b:10)"
bode9.v10.text = "When the day shall dawn upon us from on high to give light to those who sit in darkness and in the shadow of death."
bode9.v11.number = "(Ode 9b:11)"
bode9.v11.text = "To guide our feet into the way of peace."

