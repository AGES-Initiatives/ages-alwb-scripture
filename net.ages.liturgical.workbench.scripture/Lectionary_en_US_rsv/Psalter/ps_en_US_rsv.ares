psa137.v2.text = "I bow down toward thy holy temple and give thanks to thy name for thy steadfast love and thy faithfulness; for thou hast exalted above everything thy name and thy word."
psa137.v3.number = "(137:3 / rsv 138:3)"
psa137.v3.text = "On the day I called, thou didst answer me, my strength of soul thou didst increase."
psa137.v4.number = "(137:4 / rsv 138:4)"
psa137.v4.text = "All the kings of the earth shall praise thee, O LORD, for they have heard the words of thy mouth;"
psa137.v5.number = "(137:5 / rsv 138:5)"
psa137.v5.text = "And they shall sing of the ways of the LORD, for great is the glory of the LORD."
psa137.v6.number = "(137:6 / rsv 138:6)"
psa137.v6.text = "For though the LORD is high, he regards the lowly; but the haughty he knows from afar."
psa137.v7.number = "(137:7 / rsv 138:7)"
psa137.v7.text = "Though I walk in the midst of trouble, thou dost preserve my life; thou dost stretch out thy hand against the wrath of my enemies, and thy right hand delivers me."
