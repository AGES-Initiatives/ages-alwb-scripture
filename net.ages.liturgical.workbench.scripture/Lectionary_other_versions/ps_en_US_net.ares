psa114.v1.number = "(114:1)"
psa114.v1.text = "I love the Lord because he heard my plea for mercy,"
psa114.v2.number_net = "(116:2)"
psa114.v2.number = "(114:2)"
psa114.v2.text = "And listened to me. As long as I live, I will call to him when I need help."
