psa147.v4.number_net = "(147:15)"
psa147.v4.number = "(147:4)"
psa147.v4.text = "He sends his command through the earth; swiftly his order reaches its destination."
psa147.v5.number_net = "(147:16)"
psa147.v5.number = "(147:5)"
psa147.v5.text = "He sends the snow that is white like wool; he spreads the frost that is white like ashes."
psa147.v6.number_net = "(147:17)"
psa147.v6.number = "(147:6)"
psa147.v6.text = "He throws his hailstones like crumbs. Who can withstand the cold wind he sends?"
psa147.v7.number_net = "(147:18)"
psa147.v7.number = "(147:7)"
psa147.v7.text = "He then orders it all to melt; he breathes on it, and the water flows."
psa147.v8.number_net = "(147:19)"
psa147.v8.number = "(147:8)"
psa147.v8.text = "He proclaims his word to Jacob, his statutes and regulations to Israel."
psa147.v9.number_net = "(147:20)"
psa147.v9.number = "(147:9)"
psa147.v9.text = "He has not done so with any other nation; they are not aware of his regulations. Praise the Lord!"

psa148.title = "Psalm 148"
psa148.version = properties_en_US_net.version.designation
psa148.text = "Praise the Lord! Praise the Lord from the sky! Praise him in the heavens! Praise him, all his angels! Praise him, all his heavenly assembly! Praise him, O sun and moon! Praise him, all you shiny stars! Praise him, O highest heaven, and you waters above the sky! Let them praise the name of the Lord, for he gave the command and they came into existence. He established them so they would endure; he issued a decree that will not be revoked. Praise the Lord from the earth, you sea creatures and all you ocean depths, O fire and hail, snow and clouds, O stormy wind that carries out his orders, you mountains and all you hills, you fruit trees and all you cedars, you animals and all you cattle, you creeping things and birds, you kings of the earth and all you nations, you princes and all you leaders on the earth, you young men and young women, you elderly, along with you children! Let them praise the name of the Lord, for his name alone is exalted; his majesty extends over the earth and sky."
psa148.v1.number_net = "(148:1)"
psa148.v1.number = "(148:1)"
psa148.v1.text = "Praise the Lord! Praise the Lord from the sky! Praise him in the heavens!"
psa148.v1alt.text = "Praise the Lord from the sky! Praise him in the heavens! It is appropriate to sing a hymn to you, O God."
psa148.v2.number_net = "(148:2)"
psa148.v2.number = "(148:2)"
psa148.v2.text = "Praise him, all his angels! Praise him, all his heavenly assembly!"
psa148.v2alt.text = "Praise him, all his angels! Praise him, all his heavenly assembly! It is appropriate to sing a hymn to you, O God."
psa148.v3.number_net = "(148:3)"
psa148.v3.number = "(148:3)"
psa148.v3.text = "Praise him, O sun and moon! Praise him, all you shiny stars!"
psa148.v4.number_net = "(148:4)"
psa148.v4.number = "(148:4)"
psa148.v4.text = "Praise him, O highest heaven, and you waters above the sky!"
psa148.v5.number_net = "(148:5)"
psa148.v5.number = "(148:5)"
psa148.v5.text = "Let them praise the name of the Lord, for he gave the command and they came into existence."
psa148.v5a.number_net = "(148:5a)"
psa148.v5a.number = "(148:5a)"
psa148.v5a.text = "Let them praise the name of the Lord."
psa148.v5b.number_net = "(148:5b)"
psa148.v5b.number = "(148:5b)"
psa148.v5b.text = "For he gave the command and they came into existence."
psa148.v6.number_net = "(148:6)"
psa148.v6.number = "(148:6)"
psa148.v6.text = "He established them so they would endure; he issued a decree that will not be revoked."
psa148.v7.number_net = "(148:7)"
psa148.v7.number = "(148:7)"
psa148.v7.text = "Praise the Lord from the earth, you sea creatures and all you ocean depths,"
psa148.v8.number_net = "(148:8)"
psa148.v8.number = "(148:8)"
psa148.v8.text = "O fire and hail, snow and clouds, O stormy wind that carries out his orders,"
psa148.v9.number_net = "(148:9)"
psa148.v9.number = "(148:9)"
psa148.v9.text = "you mountains and all you hills, you fruit trees and all you cedars,"
psa148.v10.number_net = "(148:10)"
psa148.v10.number = "(148:10)"
psa148.v10.text = "you animals and all you cattle, you creeping things and birds,"
psa148.v11.number_net = "(148:11)"
psa148.v11.number = "(148:11)"
psa148.v11.text = "you kings of the earth and all you nations, you princes and all you leaders on the earth,"
psa148.v12.number_net = "(148:12)"
psa148.v12.number = "(148:12)"
psa148.v12.text = "you young men and young women, you elderly, along with you children!"
psa148.v13.number_net = "(148:13)"
psa148.v13.number = "(148:13)"
psa148.v13.text = "Let them praise the name of the Lord, for his name alone is exalted; his majesty extends over the earth and sky."
psa148.v13a.number_net = "(148:13a)"
psa148.v13a.number = "(148:13a)"
psa148.v13a.text = "Let them praise the name of the Lord, for his name alone is exalted."
psa148.v13b.number_net = "(148:13b)"
psa148.v13b.number = "(148:13b)"
psa148.v13b.text = "His majesty extends over the earth and sky."
psa148.v14.number_net = "(148:14)"
psa148.v14.number = "(148:14)"
psa148.v14.text = "He has made his people victorious, and given all his loyal followers reason to praise – the Israelites, the people who are close to him. Praise the Lord!"
psa148.v14a.number_net = "(148:14a)"
psa148.v14a.number = "(148:14a)"
psa148.v14a.text = "He has made his people victorious."
psa148.v14b.number_net = "(148:14b)"
psa148.v14b.number = "(148:14b)"
psa148.v14b.text = "And given all his loyal followers reason to praise – the Israelites, the people who are close to him. Praise the Lord!"

psa149.title = "Psalm 149"
psa149.version = properties_en_US_net.version.designation
psa149.text = "Praise the Lord! Sing to the Lord a new song! Praise him in the assembly of the godly! Let Israel rejoice in their Creator! Let the people of Zion delight in their King! Let them praise his name with dancing! Let them sing praises to him to the accompaniment of the tambourine and harp! For the Lord takes delight in his people; he exalts the oppressed by delivering them. Let the godly rejoice because of their vindication! Let them shout for joy upon their beds! May they praise God while they hold a two-edged sword in their hand, in order to take revenge on the nations, and punish foreigners. They bind their kings in chains, and their nobles in iron shackles, and execute the judgment to which their enemies have been sentenced. All his loyal followers will be vindicated. Praise the Lord!"
psa149.v1.number_net = "(149:1)"
psa149.v1.number = "(149:1)"
psa149.v1.text = "Praise the Lord! Sing to the Lord a new song! Praise him in the assembly of the godly!"
psa149.v2.number_net = "(149:2)"
psa149.v2.number = "(149:2)"
psa149.v2.text = "Let Israel rejoice in their Creator! Let the people of Zion delight in their King!"
psa149.v3.number_net = "(149:3)"
psa149.v3.number = "(149:3)"
psa149.v3.text = "Let them praise his name with dancing! Let them sing praises to him to the accompaniment of the tambourine and harp!"
psa149.v4.number_net = "(149:4)"
psa149.v4.number = "(149:4)"
psa149.v4.text = "For the Lord takes delight in his people; he exalts the oppressed by delivering them."
psa149.v5.number_net = "(149:5)"
psa149.v5.number = "(149:5)"
psa149.v5.text = "Let the godly rejoice because of their vindication! Let them shout for joy upon their beds!"
psa149.v6.number_net = "(149:6)"
psa149.v6.number = "(149:6)"
psa149.v6.text = "May they praise God while they hold a two-edged sword in their hand,"
psa149.v7.number_net = "(149:7)"
psa149.v7.number = "(149:7)"
psa149.v7.text = "In order to take revenge on the nations, and punish foreigners."
psa149.v8.number_net = "(149:8)"
psa149.v8.number = "(149:8)"
psa149.v8.text = "They bind their kings in chains, and their nobles in iron shackles,"
psa149.v9.number_net = "(149:9)"
psa149.v9.number = "(149:9)"
psa149.v9.text = "And execute the judgment to which their enemies have been sentenced. All his loyal followers will be vindicated. Praise the Lord!"

psa150.title = "Psalm 150"
psa150.version = properties_en_US_net.version.designation
psa150.text = "Praise the Lord! Praise God in his sanctuary! Praise him in the sky, which testifies to his strength! Praise him for his mighty acts! Praise him for his surpassing greatness! Praise him with the blast of the horn! Praise him with the lyre and the harp! Praise him with the tambourine and with dancing! Praise him with stringed instruments and the flute! Praise him with loud cymbals! Praise him with clanging cymbals! Let everything that has breath praise the Lord! Praise the Lord!"
psa150.v1.number_net = "(150:1)"
psa150.v1.number = "(150:1)"
psa150.v1.text = "Praise the Lord! Praise God in his sanctuary! Praise him in the sky, which testifies to his strength!"
psa150.v1a.number_net = "(150:1a)"
psa150.v1a.number = "(150:1a)"
psa150.v1a.text = "Praise God in his sanctuary!"
psa150.v1b.number_net = "(150:1b)"
psa150.v1b.number = "(150:1b)"
psa150.v1b.text = "Praise him in the sky, which testifies to his strength!"
psa150.v2.number_net = "(150:2)"
psa150.v2.number = "(150:2)"
psa150.v2.text = "Praise him for his mighty acts! Praise him for his surpassing greatness!"
psa150.v3.number_net = "(150:3)"
psa150.v3.number = "(150:3)"
psa150.v3.text = "Praise him with the blast of the horn! Praise him with the lyre and the harp!"
psa150.v4.number_net = "(150:4)"
psa150.v4.number = "(150:4)"
psa150.v4.text = "Praise him with the tambourine and with dancing! Praise him with stringed instruments and the flute!"
psa150.v5.number_net = "(150:5)"
psa150.v5.number = "(150:5)"
psa150.v5.text = "Praise him with loud cymbals! Praise him with clanging cymbals!"
psa150.v6.number_net = "(150:6)"
psa150.v6.number = "(150:6)"
psa150.v6.text = "Let everything that has breath praise the Lord! Praise the Lord!"
psa150.v6a.number_net = "(150:6a)"
psa150.v6a.number = "(150:6a)"
psa150.v6a.text = "Let everything that has breath praise the Lord!"
psa150.v6aalt.text = "Praise the Lord, everything that has breath!"
psa150.v6b.number_net = "(150:6b)"
psa150.v6b.number = "(150:6b)"
psa150.v6b.text = "Praise the Lord!"

