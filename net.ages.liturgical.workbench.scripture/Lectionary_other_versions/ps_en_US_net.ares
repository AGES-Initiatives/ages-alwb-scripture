psa75.v10.number = "(75:10)"
psa75.v10.text = "When God arose to execute judgment, and to deliver all the oppressed of the earth."
psa75.v11.number_net = "(76:10)"
psa75.v11.number = "(75:11)"
psa75.v11.text = "Certainly your angry judgment upon men will bring you praise; you reveal your anger in full measure."
psa75.v12.number_net = "(76:11a)"
psa75.v12.number = "(75:12)"
psa75.v12.text = "Make vows to the Lord your God and repay them! Let all those who surround him bring tribute"
