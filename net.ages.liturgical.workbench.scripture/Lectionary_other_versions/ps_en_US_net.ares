psa131.v9.text = "May your priests be clothed with integrity! May your loyal followers shout for joy!"
psa131.v9alt.text = "May your priests, O Lord, be clothed with integrity! May your loyal followers shout exuberantly!"
psa131.v10.number_net = "(132:10)"
psa131.v10.number = "(131:10)"
psa131.v10.text = "For the sake of David, your servant, do not reject your chosen king!"
psa131.v11.number_net = "(132:11)"
psa131.v11.number = "(131:11)"
psa131.v11.text = "The Lord made a reliable promise to David; he will not go back on his word. He said, ‘I will place one of your descendants on your throne."
psa131.v11a.number_net = "(132:11a)"
psa131.v11a.number = "(131:11a)"
psa131.v11a.text = "The Lord made a reliable promise to David; he will not go back on his word."
psa131.v11b.number_net = "(132:11b)"
psa131.v11b.number = "(131:11b)"
psa131.v11b.text = "‘I will place one of your descendants on your throne."
psa131.v12.number_net = "(132:12)"
psa131.v12.number = "(131:12)"
psa131.v12.text = "If your sons keep my covenant and the rules I teach them, their sons will also sit on your throne forever.’"
psa131.v12a.number_net = "(132:12a)"
psa131.v12a.number = "(131:12a)"
psa131.v12a.text = "If your sons keep my covenant and the rules I teach them,"
psa131.v12b.number_net = "(132:12b)"
psa131.v12b.number = "(131:12b)"
psa131.v12b.text = "Their sons will also sit on your throne forever.’"
psa131.v13.number_net = "(132:13)"
psa131.v13.number = "(131:13)"
psa131.v13.text = "Certainly the Lord has chosen Zion; he decided to make it his home."
psa131.v13alt.text = "The Lord has chosen Zion; he decided to make it his home."
psa131.v14.number_net = "(132:14)"
psa131.v14.number = "(131:14)"
psa131.v14.text = "He said, ‘This will be my resting place forever; I will live here, for I have chosen it."
psa131.v15.number_net = "(132:15)"
psa131.v15.number = "(131:15)"
psa131.v15.text = "I will abundantly supply what she needs; I will give her poor all the food they need."
psa131.v16.number_net = "(132:16)"
psa131.v16.number = "(131:16)"
psa131.v16.text = "I will protect her priests, and her godly people will shout exuberantly."
psa131.v17.number_net = "(132:17)"
psa131.v17.number = "(131:17)"
psa131.v17.text = "There I will make David strong; I have determined that my chosen king’s dynasty will continue."
psa131.v18.number_net = "(132:18)"
psa131.v18.number = "(131:18)"
psa131.v18.text = "I will humiliate his enemies, and his crown will shine."

psa132.title = "Psalm 132"
psa132.version = properties_en_US_net.version.designation
psa132.text = "Look! How good and how pleasant it is when brothers live together! It is like fine oil poured on the head which flows down the beard – Aaron’s beard, and then flows down his garments. It is like the dew of Hermon, which flows down upon the hills of Zion. Indeed, that is where the Lord has decreed a blessing will be available – eternal life."
psa132.v1.number_net = "(133:1)"
psa132.v1.number = "(132:1)"
psa132.v1.text = "Look! How good and how pleasant it is when brothers live together!"
