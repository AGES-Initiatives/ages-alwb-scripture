bode9.title = "Ode Nine"
bode9.version = properties_en_US_net.version.designation
bode9_mag.book_abbr = //bible_en_US_net.Luke.abbr
bode9_mag.heading1 = "Mary’s Hymn of Praise. The Magnificat."
bode9_mag.chapverse = "(1:46-55)"
bode9_mag.heading2 = "The Maiden Mother sings a hymn to her Son and God."
bode9_mag.intro = "Let us magnify in hymns the Theotokos [and Mother of the Light]."
bode9_mag.v1.number = "(9a:1)"
bode9_mag.v1.text = "My soul exalts the Lord, and my spirit has begun to rejoice in God my Savior."
bode9_mag.v2.number = "(9a:2)"
bode9_mag.v2.text = "Because he has looked upon the humble state of his servant. For from now on all generations will call me blessed."
bode9_mag.v3.number = "(9a:3)"
bode9_mag.v3.text = "Because he who is mighty has done great things for me, and holy is his name; from generation to generation he is merciful to those who fear him."
bode9_mag.v4.number = "(9a:4)"
bode9_mag.v4.text = "He has demonstrated power with his arm; he has scattered those whose pride wells up from the sheer arrogance of their hearts."
bode9_mag.v5.number = "(9a:5)"
bode9_mag.v5.text = "He has brought down the mighty from their thrones, and has lifted up those of lowly position; he has filled the hungry with good things, and has sent the rich away empty."
bode9_mag.v6.number = "(9a:6)"
bode9_mag.v6.text = "He has helped his servant Israel, remembering his mercy, as he promised to our ancestors, to Abraham and to his descendants forever."
bode9.book_abbr = //bible_en_US_net.Luke.abbr
bode9.heading1 = "Zechariah’s Praise and Prediction. The Benedictus."
bode9.chapverse = "(1:68-79)"
bode9.heading2 = "Zechariah blesses the birth of his son."
bode9.refrain = //"Blessed be the Lord God of Israel, because he has come to help and has redeemed his people."
bode9.v1.number = "(9b:1)"
bode9.v1.text = "Blessed be the Lord God of Israel, because he has come to help and has redeemed his people."
bode9.v2.number = "(9b:2)"
bode9.v2.text = "For he has raised up a horn of salvation for us in the house of his servant David."
bode9.v3.number = "(9b:3)"
bode9.v3.text = "As he spoke through the mouth of his holy prophets from long ago."
bode9.v4.number = "(9b:4)"
bode9.v4.text = "That we should be saved from our enemies, and from the hand of all who hate us."
bode9.v5.number = "(9b:5)"
bode9.v5.text = "He has done this to show mercy to our ancestors, and to remember his holy covenant."
bode9.v6.number = "(9b:6)"
bode9.v6.text = "The oath that he swore to our ancestor Abraham. This oath grants that we, being rescued from the hand of our enemies."
bode9.v7.number = "(9b:7)"
bode9.v7.text = "May serve him without fear, in holiness and righteousness before him for as long as we live."
bode9.v8.number = "(9b:8)"
bode9.v8.text = "And you, child, will be called the prophet of the Most High. For you will go before the Lord to prepare his ways."
bode9.v8a.number = "(9b:8a)"
bode9.v8a.text = "And you, child, will be called the prophet of the Most High."
bode9.v8b.number = "(9b:8b)"
bode9.v8b.text = "For you will go before the Lord to prepare his ways."
bode9.v9.number = "(9b:9)"
bode9.v9.text = "To give his people knowledge of salvation through the forgiveness of their sins, because of our God’s tender mercy."
bode9.v9a.number = "(9b:9a)"
bode9.v9a.text = "To give his people knowledge of salvation."
bode9.v9b.number = "(9b:9b)"
bode9.v9b.text = "Through the forgiveness of their sins, because of our God’s tender mercy."
bode9.v10.number = "(9b:10)"
bode9.v10.text = "The dawn will break upon us from on high to give light to those who sit in darkness and in the shadow of death."
bode9.v11.number = "(9b:11)"
bode9.v11.text = "To guide our feet into the way of peace."

