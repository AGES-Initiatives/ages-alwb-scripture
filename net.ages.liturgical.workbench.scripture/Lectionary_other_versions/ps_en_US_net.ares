psa118.v84.number_net = "(119:84)"
psa118.v84.number = "(118:84)"
psa118.v84.text = "How long must your servant endure this? When will you judge those who pursue me?"
psa118.v85.number_net = "(119:85)"
psa118.v85.number = "(118:85)"
psa118.v85.text = "The arrogant dig pits to trap me, which violates your law."
psa118.v86.number_net = "(119:86)"
psa118.v86.number = "(118:86)"
psa118.v86.text = "All your commands are reliable. I am pursued without reason. Help me!"
psa118.v87.number_net = "(119:87)"
psa118.v87.number = "(118:87)"
psa118.v87.text = "They have almost destroyed me here on the earth, but I do not reject your precepts."
psa118.v88.number_net = "(119:88)"
psa118.v88.number = "(118:88)"
psa118.v88.text = "Revive me with your loyal love, that I might keep the rules you have revealed."
psa118.v89.number_net = "(119:89)"
psa118.v89.number = "(118:89)"
psa118.v89.text = "O Lord, your instructions endure; they stand secure in heaven."
psa118.v90.number_net = "(119:90)"
psa118.v90.number = "(118:90)"
psa118.v90.text = "You demonstrate your faithfulness to all generations. You established the earth and it stood firm."
psa118.v91.number_net = "(119:91)"
psa118.v91.number = "(118:91)"
psa118.v91.text = "Today they stand firm by your decrees, for all things are your servants."
psa118.v92.number_net = "(119:92)"
psa118.v92.number = "(118:92)"
psa118.v92.text = "If I had not found encouragement in your law, I would have died in my sorrow."
psa118.v93.number_net = "(119:93)"
psa118.v93.number = "(118:93)"
psa118.v93.text = "I will never forget your precepts, for by them you have revived me."
psa118.v94.number_net = "(119:94)"
psa118.v94.number = "(118:94)"
psa118.v94.text = "I belong to you. Deliver me! For I seek your precepts."
psa118.v95.number_net = "(119:95)"
psa118.v95.number = "(118:95)"
psa118.v95.text = "The wicked prepare to kill me, yet I concentrate on your rules."
psa118.v96.number_net = "(119:96)"
psa118.v96.number = "(118:96)"
psa118.v96.text = "I realize that everything has its limits, but your commands are beyond full comprehension."
psa118.v97.number_net = "(119:97)"
psa118.v97.number = "(118:97)"
psa118.v97.text = "O how I love your law! All day long I meditate on it."
psa118.v98.number_net = "(119:98)"
psa118.v98.number = "(118:98)"
psa118.v98.text = "Your commandments make me wiser than my enemies, for I am always aware of them."
psa118.v99.number_net = "(119:99)"
psa118.v99.number = "(118:99)"
psa118.v99.text = "I have more insight than all my teachers, for I meditate on your rules."
psa118.v100.number_net = "(119:100)"
psa118.v100.number = "(118:100)"
psa118.v100.text = "I am more discerning than those older than I, for I observe your precepts."
psa118.v101.number_net = "(119:101)"
psa118.v101.number = "(118:101)"
psa118.v101.text = "I stay away from the evil path, so that I might keep your instructions."
psa118.v102.number_net = "(119:102)"
psa118.v102.number = "(118:102)"
psa118.v102.text = "I do not turn aside from your regulations, for you teach me."
psa118.v103.number_net = "(119:103)"
psa118.v103.number = "(118:103)"
psa118.v103.text = "Your words are sweeter in my mouth than honey!"
psa118.v104.number_net = "(119:104)"
psa118.v104.number = "(118:104)"
psa118.v104.text = "Your precepts give me discernment. Therefore I hate all deceitful actions."
psa118.v105.number_net = "(119:105)"
psa118.v105.number = "(118:105)"
psa118.v105.text = "Your word is a lamp to walk by, and a light to illumine my path."
psa118.v106.number_net = "(119:106)"
psa118.v106.number = "(118:106)"
psa118.v106.text = "I have vowed and solemnly sworn to keep your just regulations."
psa118.v107.number_net = "(119:107)"
psa118.v107.number = "(118:107)"
psa118.v107.text = "I am suffering terribly. O Lord, revive me with your word!"
psa118.v108.number_net = "(119:108)"
psa118.v108.number = "(118:108)"
psa118.v108.text = "O Lord, please accept the freewill offerings of my praise! Teach me your regulations!"
psa118.v109.number_net = "(119:109)"
psa118.v109.number = "(118:109)"
psa118.v109.text = "My life is in continual danger, but I do not forget your law."
psa118.v110.number_net = "(119:110)"
psa118.v110.number = "(118:110)"
psa118.v110.text = "The wicked lay a trap for me, but I do not wander from your precepts."
psa118.v111.number_net = "(119:111)"
psa118.v111.number = "(118:111)"
psa118.v111.text = "I claim your rules as my permanent possession, for they give me joy."
psa118.v112.number_net = "(119:112)"
psa118.v112.number = "(118:112)"
psa118.v112.text = "I am determined to obey your statutes at all times, to the very end."
psa118.v113.number_net = "(119:113)"
psa118.v113.number = "(118:113)"
psa118.v113.text = "I hate people with divided loyalties, but I love your law."
psa118.v114.number_net = "(119:114)"
psa118.v114.number = "(118:114)"
psa118.v114.text = "You are my hiding place and my shield. I find hope in your word."
psa118.v115.number_net = "(119:115)"
psa118.v115.number = "(118:115)"
psa118.v115.text = "Turn away from me, you evil men, so that I can observe the commands of my God."
psa118.v116.number_net = "(119:116)"
psa118.v116.number = "(118:116)"
psa118.v116.text = "Sustain me as you promised, so that I will live. Do not disappoint me!"
psa118.v117.number_net = "(119:117)"
psa118.v117.number = "(118:117)"
psa118.v117.text = "Support me, so that I will be delivered. Then I will focus on your statutes continually."
psa118.v118.number_net = "(119:118)"
psa118.v118.number = "(118:118)"
psa118.v118.text = "You despise all who stray from your statutes, for they are deceptive and unreliable."
psa118.v119.number_net = "(119:119)"
psa118.v119.number = "(118:119)"
psa118.v119.text = "You remove all the wicked of the earth like slag. Therefore I love your rules."
psa118.v120.number_net = "(119:120)"
psa118.v120.number = "(118:120)"
psa118.v120.text = "My body trembles because I fear you; I am afraid of your judgments."
psa118.v121.number_net = "(119:121)"
psa118.v121.number = "(118:121)"
psa118.v121.text = "I do what is fair and right. Do not abandon me to my oppressors!"
psa118.v122.number_net = "(119:122)"
psa118.v122.number = "(118:122)"
psa118.v122.text = "Guarantee the welfare of your servant! Do not let the arrogant oppress me!"
psa118.v123.number_net = "(119:123)"
psa118.v123.number = "(118:123)"
psa118.v123.text = "My eyes grow tired as I wait for your deliverance, for your reliable promise to be fulfilled."
psa118.v124.number_net = "(119:124)"
psa118.v124.number = "(118:124)"
psa118.v124.text = "Show your servant your loyal love! Teach me your statutes!"
psa118.v125.number_net = "(119:125)"
psa118.v125.number = "(118:125)"
psa118.v125.text = "I am your servant. Give me insight, so that I can understand your rules."
psa118.v126.number_net = "(119:126)"
psa118.v126.number = "(118:126)"
psa118.v126.text = "It is time for the Lord to act – they break your law!"
psa118.v127.number_net = "(119:127)"
psa118.v127.number = "(118:127)"
psa118.v127.text = "For this reason I love your commands more than gold, even purest gold."
psa118.v128.number_net = "(119:128)"
psa118.v128.number = "(118:128)"
psa118.v128.text = "For this reason I carefully follow all your precepts. I hate all deceitful actions."
psa118.v129.number_net = "(119:129)"
psa118.v129.number = "(118:129)"
psa118.v129.text = "Your rules are marvelous. Therefore I observe them."
psa118.v130.number_net = "(119:130)"
psa118.v130.number = "(118:130)"
psa118.v130.text = "Your instructions are a doorway through which light shines. They give insight to the untrained."
psa118.v131.number_net = "(119:131)"
psa118.v131.number = "(118:131)"
psa118.v131.text = "I open my mouth and pant, because I long for your commands."
psa118.v132.number_net = "(119:132)"
psa118.v132.number = "(118:132)"
psa118.v132.text = "Turn toward me and extend mercy to me, as you typically do to your loyal followers."
psa118.v132alt.text = "And extend mercy to me."
psa118.v133.number_net = "(119:133)"
psa118.v133.number = "(118:133)"
psa118.v133.text = "Direct my steps by your word! Do not let any sin dominate me!"
psa118.v134.number_net = "(119:134)"
psa118.v134.number = "(118:134)"
psa118.v134.text = "Deliver me from oppressive men, so that I can keep your precepts."
psa118.v135.number_net = "(119:135)"
psa118.v135.number = "(118:135)"
psa118.v135.text = "Smile on your servant! Teach me your statutes!"
psa118.v136.number_net = "(119:136)"
psa118.v136.number = "(118:136)"
psa118.v136.text = "Tears stream down from my eyes, because people do not keep your law."
psa118.v137.number_net = "(119:137)"
psa118.v137.number = "(118:137)"
psa118.v137.text = "You are just, O Lord, and your judgments are fair."
psa118.v138.number_net = "(119:138)"
psa118.v138.number = "(118:138)"
psa118.v138.text = "The rules you impose are just, and absolutely reliable."
psa118.v139.number_net = "(119:139)"
psa118.v139.number = "(118:139)"
psa118.v139.text = "My zeal consumes me, for my enemies forget your instructions."
psa118.v140.number_net = "(119:140)"
psa118.v140.number = "(118:140)"
psa118.v140.text = "Your word is absolutely pure, and your servant loves it!"
psa118.v141.number_net = "(119:141)"
psa118.v141.number = "(118:141)"
psa118.v141.text = "I am insignificant and despised, yet I do not forget your precepts."
psa118.v142.number_net = "(119:142)"
psa118.v142.number = "(118:142)"
psa118.v142.text = "Your justice endures, and your law is reliable."
psa118.v143.number_net = "(119:143)"
psa118.v143.number = "(118:143)"
psa118.v143.text = "Distress and hardship confront me, yet I find delight in your commands."
psa118.v144.number_net = "(119:144)"
psa118.v144.number = "(118:144)"
psa118.v144.text = "Your rules remain just. Give me insight so that I can live."
psa118.v145.number_net = "(119:145)"
psa118.v145.number = "(118:145)"
psa118.v145.text = "I cried out with all my heart, ‘Answer me, O Lord! I will observe your statutes.’"
psa118.v146.number_net = "(119:146)"
psa118.v146.number = "(118:146)"
psa118.v146.text = "I cried out to you, ‘Deliver me, so that I can keep your rules.’"
psa118.v147.number_net = "(119:147)"
psa118.v147.number = "(118:147)"
psa118.v147.text = "I am up before dawn crying for help. I find hope in your word."
psa118.v148.number_net = "(119:148)"
psa118.v148.number = "(118:148)"
psa118.v148.text = "My eyes anticipate the nighttime hours, so that I can meditate on your word."
psa118.v149.number_net = "(119:149)"
psa118.v149.number = "(118:149)"
psa118.v149.text = "Listen to me because of your loyal love! O Lord, revive me, as you typically do!"
psa118.v150.number_net = "(119:150)"
psa118.v150.number = "(118:150)"
psa118.v150.text = "Those who are eager to do wrong draw near; they are far from your law."
psa118.v151.number_net = "(119:151)"
psa118.v151.number = "(118:151)"
psa118.v151.text = "You are near, O Lord, and all your commands are reliable."
psa118.v152.number_net = "(119:152)"
psa118.v152.number = "(118:152)"
psa118.v152.text = "I learned long ago that you ordained your rules to last."
psa118.v153.number_net = "(119:153)"
psa118.v153.number = "(118:153)"
psa118.v153.text = "See my pain and rescue me! For I do not forget your law."
psa118.v154.number_net = "(119:154)"
psa118.v154.number = "(118:154)"
psa118.v154.text = "Fight for me and defend me! Revive me with your word!"
psa118.v155.number_net = "(119:155)"
psa118.v155.number = "(118:155)"
psa118.v155.text = "The wicked have no chance for deliverance, for they do not seek your statutes."
psa118.v156.number_net = "(119:156)"
psa118.v156.number = "(118:156)"
psa118.v156.text = "Your compassion is great, O Lord. Revive me, as you typically do!"
psa118.v157.number_net = "(119:157)"
psa118.v157.number = "(118:157)"
psa118.v157.text = "The enemies who chase me are numerous. Yet I do not turn aside from your rules."
psa118.v158.number_net = "(119:158)"
psa118.v158.number = "(118:158)"
psa118.v158.text = "I take note of the treacherous and despise them, because they do not keep your instructions."
psa118.v159.number_net = "(119:159)"
psa118.v159.number = "(118:159)"
psa118.v159.text = "See how I love your precepts! O Lord, revive me with your loyal love!"
psa118.v160.number_net = "(119:160)"
psa118.v160.number = "(118:160)"
psa118.v160.text = "Your instructions are totally reliable; all your just regulations endure."
psa118.v161.number_net = "(119:161)"
psa118.v161.number = "(118:161)"
psa118.v161.text = "Rulers pursue me for no reason, yet I am more afraid of disobeying your instructions."
psa118.v162.number_net = "(119:162)"
psa118.v162.number = "(118:162)"
psa118.v162.text = "I rejoice in your instructions, like one who finds much plunder."
psa118.v163.number_net = "(119:163)"
psa118.v163.number = "(118:163)"
psa118.v163.text = "I hate and despise deceit; I love your law."
psa118.v164.number_net = "(119:164)"
psa118.v164.number = "(118:164)"
psa118.v164.text = "Seven times a day I praise you because of your just regulations."
psa118.v165.number_net = "(119:165)"
psa118.v165.number = "(118:165)"
psa118.v165.text = "Those who love your law are completely secure; nothing causes them to stumble."
psa118.v166.number_net = "(119:166)"
psa118.v166.number = "(118:166)"
psa118.v166.text = "I hope for your deliverance, O Lord, and I obey your commands."
psa118.v167.number_net = "(119:167)"
psa118.v167.number = "(118:167)"
psa118.v167.text = "I keep your rules; I love them greatly."
psa118.v168.number_net = "(119:168)"
psa118.v168.number = "(118:168)"
psa118.v168.text = "I keep your precepts and rules, for you are aware of everything I do."
psa118.v169.number_net = "(119:169)"
psa118.v169.number = "(118:169)"
psa118.v169.text = "Listen to my cry for help, O Lord! Give me insight by your word!"
psa118.v170.number_net = "(119:170)"
psa118.v170.number = "(118:170)"
psa118.v170.text = "Listen to my appeal for mercy! Deliver me, as you promised."
psa118.v171.number_net = "(119:171)"
psa118.v171.number = "(118:171)"
psa118.v171.text = "May praise flow freely from my lips, for you teach me your statutes."
psa118.v172.number_net = "(119:172)"
psa118.v172.number = "(118:172)"
psa118.v172.text = "May my tongue sing about your instructions, for all your commands are just."
psa118.v173.number_net = "(119:173)"
psa118.v173.number = "(118:173)"
psa118.v173.text = "May your hand help me, for I choose to obey your precepts."
psa118.v174.number_net = "(119:174)"
psa118.v174.number = "(118:174)"
psa118.v174.text = "I long for your deliverance, O Lord; I find delight in your law."
psa118.v175.number_net = "(119:175)"
psa118.v175.number = "(118:175)"
psa118.v175.text = "May I live and praise you! May your regulations help me!"
psa118.v176.number_net = "(119:176)"
psa118.v176.number = "(118:176)"
psa118.v176.text = "I have wandered off like a lost sheep. Come looking for your servant, for I do not forget your commands."

kathisma_18.title = "KATHISMA 18."

psa119.title = "Psalm 119"
psa119.version = properties_en_US_net.version.designation
psa119.text = "In my distress I cried out to the Lord and he answered me. I said, ‘O Lord, rescue me from those who lie with their lips and those who deceive with their tongue.’ How will he severely punish you, you deceptive talker? Here’s how! With the sharp arrows of warriors, with arrowheads forged over the hot coals. How miserable I am! For I have lived temporarily in Meshech; I have resided among the tents of Kedar. For too long I have had to reside with those who hate peace. I am committed to peace, but when I speak, they want to make war."
psa119.v1.number_net = "(120:1)"
psa119.v1.number = "(119:1)"
psa119.v1.text = "In my distress I cried out to the Lord and he answered me."
psa119.v2.number_net = "(120:2)"
psa119.v2.number = "(119:2)"
psa119.v2.text = "I said, ‘O Lord, rescue me from those who lie with their lips and those who deceive with their tongue.’"
psa119.v3.number_net = "(120:3)"
psa119.v3.number = "(119:3)"
psa119.v3.text = "How will he severely punish you, you deceptive talker?"
psa119.v4.number_net = "(120:4)"
psa119.v4.number = "(119:4)"
psa119.v4.text = "Here’s how! With the sharp arrows of warriors, with arrowheads forged over the hot coals."
psa119.v5.number_net = "(120:5)"
psa119.v5.number = "(119:5)"
psa119.v5.text = "How miserable I am! For I have lived temporarily in Meshech; I have resided among the tents of Kedar."
psa119.v6.number_net = "(120:6)"
psa119.v6.number = "(119:6)"
psa119.v6.text = "For too long I have had to reside with those who hate peace."
psa119.v7.number_net = "(120:7)"
psa119.v7.number = "(119:7)"
psa119.v7.text = "I am committed to peace, but when I speak, they want to make war."

psa120.title = "Psalm 120"
psa120.version = properties_en_US_net.version.designation
psa120.text = "I look up toward the hills. From where does my help come? My help comes from the Lord, the Creator of heaven and earth! May he not allow your foot to slip! May your protector not sleep! Look! Israel’s protector does not sleep or slumber! The Lord is your protector; the Lord is the shade at your right hand. The sun will not harm you by day, or the moon by night. The Lord will protect you from all harm; he will protect your life. The Lord will protect you in all you do, now and forevermore."
psa120.v1.number_net = "(121:1)"
psa120.v1.number = "(120:1)"
psa120.v1.text = "I look up toward the hills. From where does my help come?"
psa120.v2.number_net = "(121:2)"
psa120.v2.number = "(120:2)"
psa120.v2.text = "My help comes from the Lord, the Creator of heaven and earth!"
psa120.v3.number_net = "(121:3)"
psa120.v3.number = "(120:3)"
psa120.v3.text = "May he not allow your foot to slip! May your protector not sleep!"
psa120.v4.number_net = "(121:4)"
psa120.v4.number = "(120:4)"
psa120.v4.text = "Look! Israel’s protector does not sleep or slumber!"
psa120.v5.number_net = "(121:5)"
psa120.v5.number = "(120:5)"
psa120.v5.text = "The Lord is your protector; the Lord is the shade at your right hand."
psa120.v6.number_net = "(121:6)"
psa120.v6.number = "(120:6)"
psa120.v6.text = "The sun will not harm you by day, or the moon by night."
psa120.v7.number_net = "(121:7)"
psa120.v7.number = "(120:7)"
psa120.v7.text = "The Lord will protect you from all harm; he will protect your life."
psa120.v8.number_net = "(121:8)"
psa120.v8.number = "(120:8)"
psa120.v8.text = "The Lord will protect you in all you do, now and forevermore."

psa121.title = "Psalm 121"
psa121.version = properties_en_US_net.version.designation
psa121.text = "I was glad because they said to me, ‘We will go to the Lord’s temple.’ Our feet are standing inside your gates, O Jerusalem. Jerusalem is a city designed to accommodate an assembly. The tribes go up there, the tribes of the Lord, where it is required that Israel give thanks to the name of the Lord. Indeed, the leaders sit there on thrones and make legal decisions, on the thrones of the house of David. Pray for the peace of Jerusalem! May those who love her prosper! May there be peace inside your defenses, and prosperity inside your fortresses! For the sake of my brothers and my neighbors I will say, ‘May there be peace in you!’ For the sake of the temple of the Lord our God I will pray for you to prosper."
psa121.v1.number_net = "(122:1)"
psa121.v1.number = "(121:1)"
psa121.v1.text = "I was glad because they said to me, ‘We will go to the Lord’s temple.’"
psa121.v2.number_net = "(122:2)"
psa121.v2.number = "(121:2)"
psa121.v2.text = "Our feet are standing inside your gates, O Jerusalem."
psa121.v3.number_net = "(122:3)"
psa121.v3.number = "(121:3)"
psa121.v3.text = "Jerusalem is a city designed to accommodate an assembly."
psa121.v4.number_net = "(122:4)"
psa121.v4.number = "(121:4)"
psa121.v4.text = "The tribes go up there, the tribes of the Lord, where it is required that Israel give thanks to the name of the Lord."
psa121.v5.number_net = "(122:5)"
psa121.v5.number = "(121:5)"
psa121.v5.text = "Indeed, the leaders sit there on thrones and make legal decisions, on the thrones of the house of David."
psa121.v6.number_net = "(122:6)"
psa121.v6.number = "(121:6)"
psa121.v6.text = "Pray for the peace of Jerusalem! May those who love her prosper!"
psa121.v7.number_net = "(122:7)"
psa121.v7.number = "(121:7)"
psa121.v7.text = "May there be peace inside your defenses, and prosperity inside your fortresses!"
psa121.v8.number_net = "(122:8)"
psa121.v8.number = "(121:8)"
psa121.v8.text = "For the sake of my brothers and my neighbors I will say, ‘May there be peace in you!’"
psa121.v9.number_net = "(122:9)"
psa121.v9.number = "(121:9)"
psa121.v9.text = "For the sake of the temple of the Lord our God I will pray for you to prosper."

psa122.title = "Psalm 122"
psa122.version = properties_en_US_net.version.designation
psa122.text = "I look up toward you, the one enthroned in heaven. Look, as the eyes of servants look to the hand of their master, as the eyes of a female servant look to the hand of her mistress, so my eyes will look to the Lord, our God, until he shows us favor. Show us favor, O Lord, show us favor! For we have had our fill of humiliation, and then some. We have had our fill of the taunts of the self-assured, of the contempt of the proud."
psa122.v1.number_net = "(123:1)"
psa122.v1.number = "(122:1)"
psa122.v1.text = "I look up toward you, the one enthroned in heaven."
psa122.v2.number_net = "(123:2)"
psa122.v2.number = "(122:2)"
psa122.v2.text = "Look, as the eyes of servants look to the hand of their master, as the eyes of a female servant look to the hand of her mistress, so my eyes will look to the Lord, our God, until he shows us favor."
psa122.v3.number_net = "(123:3)"
psa122.v3.number = "(122:3)"
psa122.v3.text = "Show us favor, O Lord, show us favor! For we have had our fill of humiliation, and then some."
psa122.v4.number_net = "(123:4)"
psa122.v4.number = "(122:4)"
psa122.v4.text = "We have had our fill of the taunts of the self-assured, of the contempt of the proud."

psa123.title = "Psalm 123"
psa123.version = properties_en_US_net.version.designation
psa123.text = "If the Lord had not been on our side – let Israel say this! – if the Lord had not been on our side, when men attacked us, they would have swallowed us alive, when their anger raged against us. The water would have overpowered us; the current would have overwhelmed us. The raging water would have overwhelmed us. The Lord deserves praise, for he did not hand us over as prey to their teeth. We escaped with our lives, like a bird from a hunter’s snare. The snare broke, and we escaped. Our deliverer is the Lord, the Creator of heaven and earth."
psa123.v1.number_net = "(124:1)"
psa123.v1.number = "(123:1)"
psa123.v1.text = "If the Lord had not been on our side – let Israel say this!"
psa123.v2.number_net = "(124:2)"
psa123.v2.number = "(123:2)"
psa123.v2.text = "If the Lord had not been on our side, when men attacked us,"
psa123.v3.number_net = "(124:3)"
psa123.v3.number = "(123:3)"
psa123.v3.text = "They would have swallowed us alive, when their anger raged against us."
psa123.v4.number_net = "(124:4)"
psa123.v4.number = "(123:4)"
psa123.v4.text = "The water would have overpowered us; the current would have overwhelmed us."
psa123.v5.number_net = "(124:5)"
psa123.v5.number = "(123:5)"
psa123.v5.text = "The raging water would have overwhelmed us."
psa123.v6.number_net = "(124:6)"
psa123.v6.number = "(123:6)"
psa123.v6.text = "The Lord deserves praise, for he did not hand us over as prey to their teeth."
psa123.v7.number_net = "(124:7)"
psa123.v7.number = "(123:7)"
psa123.v7.text = "We escaped with our lives, like a bird from a hunter’s snare. The snare broke, and we escaped."
psa123.v8.number_net = "(124:8)"
psa123.v8.number = "(123:8)"
psa123.v8.text = "Our deliverer is the Lord, the Creator of heaven and earth."

psa124.title = "Psalm 124"
psa124.version = properties_en_US_net.version.designation
psa124.text = "Those who trust in the Lord are like Mount Zion; it cannot be upended and will endure forever. As the mountains surround Jerusalem, so the Lord surrounds his people, now and forevermore. Indeed, the scepter of a wicked king will not settle upon the allotted land of the godly. Otherwise the godly might do what is wrong. Do good, O Lord, to those who are good, to the morally upright! As for those who are bent on traveling a sinful path, may the Lord remove them, along with those who behave wickedly! May Israel experience peace!"
psa124.v1.number_net = "(125:1)"
psa124.v1.number = "(124:1)"
psa124.v1.text = "Those who trust in the Lord are like Mount Zion; it cannot be upended and will endure forever."
psa124.v1a.number_net = "(125:1a)"
psa124.v1a.number = "(124:1a)"
psa124.v1a.text = "Those who trust in the Lord are like Mount Zion."
psa124.v1b.number_net = "(125:1b)"
psa124.v1b.number = "(124:1b)"
psa124.v1b.text = "It cannot be upended and will endure forever."
psa124.v2.number_net = "(125:2)"
psa124.v2.number = "(124:2)"
psa124.v2.text = "As the mountains surround Jerusalem, so the Lord surrounds his people, now and forevermore."
psa124.v3.number_net = "(125:3)"
psa124.v3.number = "(124:3)"
psa124.v3.text = "Indeed, the scepter of a wicked king will not settle upon the allotted land of the godly. Otherwise the godly might do what is wrong."
psa124.v4.number_net = "(125:4)"
psa124.v4.number = "(124:4)"
psa124.v4.text = "Do good, O Lord, to those who are good, to the morally upright!"
psa124.v5.number_net = "(125:5)"
psa124.v5.number = "(124:5)"
psa124.v5.text = "As for those who are bent on traveling a sinful path, may the Lord remove them, along with those who behave wickedly! May Israel experience peace!"

psa125.title = "Psalm 125"
psa125.version = properties_en_US_net.version.designation
psa125.text = "When the Lord restored the well-being of Zion, we thought we were dreaming. At that time we laughed loudly and shouted for joy. At that time the nations said, ‘The Lord has accomplished great things for these people.’ The Lord did indeed accomplish great things for us. We were happy. O Lord, restore our well-being, just as the streams in the arid south are replenished. Those who shed tears as they plant will shout for joy when they reap the harvest. The one who weeps as he walks along, carrying his bag of seed, will certainly come in with a shout of joy, carrying his sheaves of grain."
psa125.v1.number_net = "(126:1)"
psa125.v1.number = "(125:1)"
psa125.v1.text = "When the Lord restored the well-being of Zion, we thought we were dreaming."
psa125.v2.number_net = "(126:2)"
psa125.v2.number = "(125:2)"
psa125.v2.text = "At that time we laughed loudly and shouted for joy. At that time the nations said, ‘The Lord has accomplished great things for these people.’"
psa125.v2a.number_net = "(126:2a)"
psa125.v2a.number = "(125:2a)"
psa125.v2a.text = "At that time we laughed loudly and shouted for joy."
psa125.v2b.number_net = "(126:2b)"
psa125.v2b.number = "(125:2b)"
psa125.v2b.text = "At that time the nations said, ‘The Lord has accomplished great things for these people.’"
psa125.v3.number_net = "(126:3)"
psa125.v3.number = "(125:3)"
psa125.v3.text = "The Lord did indeed accomplish great things for us. We were happy."
psa125.v4.number_net = "(126:4)"
psa125.v4.number = "(125:4)"
psa125.v4.text = "O Lord, restore our well-being, just as the streams in the arid south are replenished."
psa125.v5.number_net = "(126:5)"
psa125.v5.number = "(125:5)"
psa125.v5.text = "Those who shed tears as they plant will shout for joy when they reap the harvest."
psa125.v6.number_net = "(126:6)"
psa125.v6.number = "(125:6)"
psa125.v6.text = "The one who weeps as he walks along, carrying his bag of seed, will certainly come in with a shout of joy, carrying his sheaves of grain."
psa125.v6a.number_net = "(126:6a)"
psa125.v6a.number = "(125:6a)"
psa125.v6a.text = "The one who weeps as he walks along, carrying his bag of seed,"
psa125.v6b.number_net = "(126:6b)"
psa125.v6b.number = "(125:6b)"
psa125.v6b.text = "will certainly come in with a shout of joy, carrying his sheaves of grain."

psa126.title = "Psalm 126"
psa126.version = properties_en_US_net.version.designation
psa126.text = "If the Lord does not build a house, then those who build it work in vain. If the Lord does not guard a city, then the watchman stands guard in vain. It is vain for you to rise early, come home late, and work so hard for your food. Yes, he can provide for those whom he loves even when they sleep. Yes, sons are a gift from the Lord, the fruit of the womb is a reward. Sons born during one’s youth are like arrows in a warrior’s hand. How blessed is the man who fills his quiver with them! They will not be put to shame when they confront enemies at the city gate."
psa126.v1.number_net = "(127:1)"
psa126.v1.number = "(126:1)"
psa126.v1.text = "If the Lord does not build a house, then those who build it work in vain. If the Lord does not guard a city, then the watchman stands guard in vain."
psa126.v1a.number_net = "(127:1a)"
psa126.v1a.number = "(126:1a)"
psa126.v1a.text = "If the Lord does not build a house, then those who build it work in vain."
psa126.v1b.number_net = "(127:1b)"
psa126.v1b.number = "(126:1b)"
psa126.v1b.text = "If the Lord does not guard a city, then the watchman stands guard in vain."
psa126.v2.number_net = "(127:2)"
psa126.v2.number = "(126:2)"
psa126.v2.text = "It is vain for you to rise early, come home late, and work so hard for your food. Yes, he can provide for those whom he loves even when they sleep."
psa126.v3.number_net = "(127:3)"
psa126.v3.number = "(126:3)"
psa126.v3.text = "Yes, sons are a gift from the Lord, the fruit of the womb is a reward."
psa126.v4.number_net = "(127:4)"
psa126.v4.number = "(126:4)"
psa126.v4.text = "Sons born during one’s youth are like arrows in a warrior’s hand."
psa126.v5.number_net = "(127:5)"
psa126.v5.number = "(126:5)"
psa126.v5.text = "How blessed is the man who fills his quiver with them! They will not be put to shame when they confront enemies at the city gate."

psa127.title = "Psalm 127"
psa127.version = properties_en_US_net.version.designation
psa127.text = "How blessed is every one of the Lord’s loyal followers, each one who keeps his commands! You will eat what you worked so hard to grow. You will be blessed and secure. Your wife will be like a fruitful vine in the inner rooms of your house; your children will be like olive branches, as they sit all around your table. Yes indeed, the man who fears the Lord will be blessed in this way. May the Lord bless you from Zion, that you might see Jerusalem prosper all the days of your life, and that you might see your grandchildren. May Israel experience peace!"
psa127.v1.number_net = "(128:1)"
psa127.v1.number = "(127:1)"
psa127.v1.text = "How blessed is every one of the Lord’s loyal followers, each one who keeps his commands!"
psa127.v2.number_net = "(128:2)"
psa127.v2.number = "(127:2)"
psa127.v2.text = "You will eat what you worked so hard to grow. You will be blessed and secure."
psa127.v3.number_net = "(128:3)"
psa127.v3.number = "(127:3)"
psa127.v3.text = "Your wife will be like a fruitful vine in the inner rooms of your house; your children will be like olive branches, as they sit all around your table."
psa127.v4.number_net = "(128:4)"
psa127.v4.number = "(127:4)"
psa127.v4.text = "Yes indeed, the man who fears the Lord will be blessed in this way."
psa127.v5.number_net = "(128:5)"
psa127.v5.number = "(127:5)"
psa127.v5.text = "May the Lord bless you from Zion, that you might see Jerusalem prosper all the days of your life,"
psa127.v6.number_net = "(128:6)"
psa127.v6.number = "(127:6)"
psa127.v6.text = "And that you might see your grandchildren. May Israel experience peace!"

psa128.title = "Psalm 128"
psa128.version = properties_en_US_net.version.designation
psa128.text = "‘Since my youth they have often attacked me,’ let Israel say. ‘Since my youth they have often attacked me, but they have not defeated me. The plowers plowed my back; they made their furrows long. The Lord is just; he cut the ropes of the wicked.’ May all who hate Zion be humiliated and turned back! May they be like the grass on the rooftops which withers before one can even pull it up, which cannot fill the reaper’s hand, or the lap of the one who gathers the grain! Those who pass by will not say, ‘May you experience the Lord’s blessing! We pronounce a blessing on you in the name of the Lord.’"
psa128.v1.number_net = "(129:1)"
psa128.v1.number = "(128:1)"
psa128.v1.text = "‘Since my youth they have often attacked me,’ let Israel say."
psa128.v2.number_net = "(129:2)"
psa128.v2.number = "(128:2)"
psa128.v2.text = "‘Since my youth they have often attacked me, but they have not defeated me."
psa128.v3.number_net = "(129:3)"
psa128.v3.number = "(128:3)"
psa128.v3.text = "The plowers plowed my back; they made their furrows long."
psa128.v4.number_net = "(129:4)"
psa128.v4.number = "(128:4)"
psa128.v4.text = "The Lord is just; he cut the ropes of the wicked.’"
psa128.v5.number_net = "(129:5)"
psa128.v5.number = "(128:5)"
psa128.v5.text = "May all who hate Zion be humiliated and turned back!"
psa128.v6.number_net = "(129:6)"
psa128.v6.number = "(128:6)"
psa128.v6.text = "May they be like the grass on the rooftops which withers before one can even pull it up,"
psa128.v7.number_net = "(129:7)"
psa128.v7.number = "(128:7)"
psa128.v7.text = "Which cannot fill the reaper’s hand, or the lap of the one who gathers the grain!"
psa128.v8.number_net = "(129:8)"
psa128.v8.number = "(128:8)"
psa128.v8.text = "Those who pass by will not say, ‘May you experience the Lord’s blessing! We pronounce a blessing on you in the name of the Lord.’"
psa128.v8a.number_net = "(129:8a)"
psa128.v8a.number = "(128:8a)"
psa128.v8a.text = "Those who pass by will not say, ‘May you experience the Lord’s blessing!"
psa128.v8b.number_net = "(129:8b)"
psa128.v8b.number = "(128:8b)"
psa128.v8b.text = "We pronounce a blessing on you in the name of the Lord.’"

psa129.title = "Psalm 129"
psa129.version = properties_en_US_net.version.designation
psa129.text = "From the deep water I cry out to you, O Lord. O Lord, listen to me! Pay attention to my plea for mercy! If you, O Lord, were to keep track of sins, O Lord, who could stand before you? But you are willing to forgive, so that you might be honored. I rely on the Lord, I rely on him with my whole being; I wait for his assuring word. I yearn for the Lord, more than watchmen do for the morning, yes, more than watchmen do for the morning. O Israel, hope in the Lord, for the Lord exhibits loyal love, and is more than willing to deliver. He will deliver Israel from all the consequences of their sins."
psa129.v1.number_net = "(130:1)"
psa129.v1.number = "(129:1)"
psa129.v1.text = "From the deep water I cry out to you, O Lord."
psa129.v2.number_net = "(130:2)"
psa129.v2.number = "(129:2)"
psa129.v2.text = "O Lord, listen to me! Pay attention to my plea for mercy!"
psa129.v2a.number_net = "(130:2a)"
psa129.v2a.number = "(129:2a)"
psa129.v2a.text = "O Lord, listen to me!"
psa129.v2b.number_net = "(130:2b)"
psa129.v2b.number = "(129:2b)"
psa129.v2b.text = "Pay attention to my plea for mercy!"
psa129.v3.number_net = "(130:3)"
psa129.v3.number = "(129:3)"
psa129.v3.text = "If you, O Lord, were to keep track of sins, O Lord, who could stand before you?"
psa129.v4.number_net = "(130:4)"
psa129.v4.number = "(129:4)"
psa129.v4.text = "But you are willing to forgive, so that you might be honored."
psa129.v5.number_net = "(130:5)"
psa129.v5.number = "(129:5)"
psa129.v5.text = "I rely on the Lord, I rely on him with my whole being; I wait for his assuring word."
psa129.v6.number_net = "(130:6-7a)"
psa129.v6.number = "(129:6)"
psa129.v6.text = "I yearn for the Lord, more than watchmen do for the morning, yes, more than watchmen do for the morning. O Israel, hope in the Lord,"
psa129.v6a.number_net = "(130:6a)"
psa129.v6a.number = "(129:6a)"
psa129.v6a.text = "I yearn for the Lord."
psa129.v6b.number_net = "(130:6b-7a)"
psa129.v6b.number = "(129:6b)"
psa129.v6b.text = "More than watchmen do for the morning, yes, more than watchmen do for the morning. O Israel, hope in the Lord."
psa129.v7.number_net = "(130:7b)"
psa129.v7.number = "(129:7)"
psa129.v7.text = "For the Lord exhibits loyal love, and is more than willing to deliver."
psa129.v8.number_net = "(130:8)"
psa129.v8.number = "(129:8)"
psa129.v8.text = "He will deliver Israel from all the consequences of their sins."

psa130.title = "Psalm 130"
psa130.version = properties_en_US_net.version.designation
psa130.text = "O Lord, my heart is not proud, nor do I have a haughty look. I do not have great aspirations, or concern myself with things that are beyond me. Indeed I am composed and quiet, like a young child carried by its mother; I am content like the young child I carry. O Israel, hope in the Lord now and forevermore!"
psa130.v1.number_net = "(131:1)"
psa130.v1.number = "(130:1)"
psa130.v1.text = "O Lord, my heart is not proud, nor do I have a haughty look. I do not have great aspirations, or concern myself with things that are beyond me."
psa130.v1a.number_net = "(131:1a)"
psa130.v1a.number = "(130:1a)"
psa130.v1a.text = "O Lord, my heart is not proud, nor do I have a haughty look."
psa130.v1b.number_net = "(131:1b)"
psa130.v1b.number = "(130:1b)"
psa130.v1b.text = "I do not have great aspirations, or concern myself with things that are beyond me."
psa130.v2.number_net = "(131:2)"
psa130.v2.number = "(130:2)"
psa130.v2.text = "Indeed I am composed and quiet, like a young child carried by its mother; I am content like the young child I carry."
psa130.v3.number_net = "(131:3)"
psa130.v3.number = "(130:3)"
psa130.v3.text = "O Israel, hope in the Lord now and forevermore!"

psa131.title = "Psalm 131"
psa131.version = properties_en_US_net.version.designation
psa131.text = "O Lord, for David’s sake remember all his strenuous effort, and how he made a vow to the Lord, and swore an oath to the powerful Ruler of Jacob. He said, ‘I will not enter my own home, or get into my bed. I will not allow my eyes to sleep, or my eyelids to slumber, until I find a place for the Lord, a fine dwelling place for the powerful Ruler of Jacob.’ Look, we heard about it in Ephrathah, we found it in the territory of Jaar. Let us go to his dwelling place! Let us worship before his footstool! Ascend, O Lord, to your resting place, you and the ark of your strength! May your priests be clothed with integrity! May your loyal followers shout for joy! For the sake of David, your servant, do not reject your chosen king! The Lord made a reliable promise to David; he will not go back on his word. He said, ‘I will place one of your descendants on your throne. If your sons keep my covenant and the rules I teach them, their sons will also sit on your throne forever.’ Certainly the Lord has chosen Zion; he decided to make it his home. He said, ‘This will be my resting place forever; I will live here, for I have chosen it. I will abundantly supply what she needs; I will give her poor all the food they need. I will protect her priests, and her godly people will shout exuberantly. There I will make David strong; I have determined that my chosen king’s dynasty will continue. I will humiliate his enemies, and his crown will shine."
psa131.v1.number_net = "(132:1)"
psa131.v1.number = "(131:1)"
psa131.v1.text = "O Lord, for David’s sake remember all his strenuous effort,"
psa131.v1b.number_net = "(132:1b)"
psa131.v1b.number = "(131:1b)"
psa131.v1b.text = "His strenuous effort."
psa131.v2.number_net = "(132:2)"
psa131.v2.number = "(131:2)"
psa131.v2.text = "And how he made a vow to the Lord, and swore an oath to the powerful Ruler of Jacob."
psa131.v3.number_net = "(132:3)"
psa131.v3.number = "(131:3)"
psa131.v3.text = "He said, ‘I will not enter my own home, or get into my bed."
psa131.v4.number_net = "(132:4)"
psa131.v4.number = "(131:4)"
psa131.v4.text = "I will not allow my eyes to sleep, or my eyelids to slumber,"
psa131.v5.number_net = "(132:5)"
psa131.v5.number = "(131:5)"
psa131.v5.text = "Until I find a place for the Lord, a fine dwelling place for the powerful Ruler of Jacob.’"
psa131.v6.number_net = "(132:6)"
psa131.v6.number = "(131:6)"
psa131.v6.text = "Look, we heard about it in Ephrathah, we found it in the territory of Jaar."
psa131.v6a.number_net = "(132:6a)"
psa131.v6a.number = "(131:6a)"
psa131.v6a.text = "Look, we heard about it in Ephrathah."
psa131.v6b.number_net = "(132:6b)"
psa131.v6b.number = "(131:6b)"
psa131.v6b.text = "We found it in the territory of Jaar."
psa131.v7.number_net = "(132:7)"
psa131.v7.number = "(131:7)"
psa131.v7.text = "Let us go to his dwelling place! Let us worship before his footstool!"
psa131.v8.number_net = "(132:8)"
psa131.v8.number = "(131:8)"
psa131.v8.text = "Ascend, O Lord, to your resting place, you and the ark of your strength!"
psa131.v9.number_net = "(132:9)"
