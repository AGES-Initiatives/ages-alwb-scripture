bode1.title = "Ode One"
bode1.version = properties_en_US_net.version.designation
bode1.book_abbr = //bible_en_US_net.Exodus.abbr
bode1.heading1 = "The Song of Moses"
bode1.chapverse = "(15:1-19)"
bode1.heading2 = "Then Moses sang this song to the Lord."
bode1.refrain = "To the Lord I will sing, for he has triumphed gloriously."
bode1.v1.number_net = "(1:1)"
bode1.v1.number = "(1:1)"
bode1.v1.text = "I will sing to the Lord, for he has triumphed gloriously, the horse and its rider he has thrown into the sea."
bode1.v2.number_net = "(1:2)"
bode1.v2.number = "(1:2)"
bode1.v2.text = "The Lord is my strength and my song, and he has become my salvation. This is my God, and I will praise him, my father’s God, and I will exalt him."
bode1.v3.number_net = "(1:3)"
bode1.v3.number = "(1:3)"
bode1.v3.text = "The Lord is a warrior, the Lord is his name."
bode1.v4.number_net = "(1:4)"
bode1.v4.number = "(1:4)"
bode1.v4.text = "The chariots of Pharaoh and his army he has thrown into the sea, and his chosen officers were drowned in the Red Sea."
bode1.v4a.number_net = "(1:4a)"
bode1.v4a.number = "(1:4a)"
bode1.v4a.text = "The chariots of Pharaoh and his army he has thrown into the sea."
bode1.v5.number_net = "(1:5)"
bode1.v5.number = "(1:5)"
bode1.v5.text = "The depths have covered them, they went down to the bottom like a stone."
bode1.v6.number_net = "(1:6-7a)"
bode1.v6.number = "(1:6)"
bode1.v6.text = "Your right hand, O Lord, was majestic in power, your right hand, O Lord, shattered the enemy. In the abundance of your majesty you have overthrown those who rise up against you."
bode1.v7.number_net = "(1:7b)"
bode1.v7.number = "(1:7)"
bode1.v7.text = "You sent forth your wrath; it consumed them like stubble."
bode1.v8.number_net = "(1:8)"
bode1.v8.number = "(1:8)"
bode1.v8.text = "By the blast of your nostrils the waters were piled up, the flowing water stood upright like a heap, and the deep waters were solidified in the heart of the sea."
bode1.v9.number_net = "(1:9)"
bode1.v9.number = "(1:9)"
bode1.v9.text = "The enemy said, ‘I will chase, I will overtake, I will divide the spoil; my desire will be satisfied on them. I will draw my sword, my hand will destroy them.’"
bode1.v10.number_net = "(1:10)"
bode1.v10.number = "(1:10)"
bode1.v10.text = "But you blew with your breath, and the sea covered them. They sank like lead in the mighty waters."
bode1.v11.number_net = "(1:11)"
bode1.v11.number = "(1:11)"
bode1.v11.text = "Who is like you, O Lord, among the gods? Who is like you? – majestic in holiness, fearful in praises, working wonders?"
bode1.v12.number_net = "(1:12)"
bode1.v12.number = "(1:12)"
bode1.v12.text = "You stretched out your right hand, the earth swallowed them."
bode1.v13.number_net = "(1:13)"
bode1.v13.number = "(1:13)"
bode1.v13.text = "By your loyal love you will lead the people whom you have redeemed; you will guide them by your strength to your holy dwelling place."
bode1.v14.number_net = "(1:14)"
bode1.v14.number = "(1:14)"
bode1.v14.text = "The nations will hear and tremble; anguish will seize the inhabitants of Philistia."
bode1.v15.number_net = "(1:15)"
bode1.v15.number = "(1:15)"
bode1.v15.text = "Then the chiefs of Edom will be terrified, trembling will seize the leaders of Moab, and the inhabitants of Canaan will shake."
bode1.v16.number_net = "(1:16a)"
bode1.v16.number = "(1:16)"
bode1.v16.text = "Fear and dread will fall on them; by the greatness of your arm they will be as still as stone"
bode1.v17.number_net = "(1:16b)"
bode1.v17.number = "(1:17)"
bode1.v17.text = "until your people pass by, O Lord, until the people whom you have bought pass by."
bode1.v18.number_net = "(1:17)"
bode1.v18.number = "(1:18)"
bode1.v18.text = "You will bring them in and plant them in the mountain of your inheritance, in the place you made for your residence, O Lord, the sanctuary, O Lord, that your hands have established."
bode1.v18b.number_net = "(1:17b)"
bode1.v18b.number = "(1:18b)"
bode1.v18b.text = "the sanctuary, O Lord, that your hands have established."
bode1.v19.number_net = "(1:18)"
bode1.v19.number = "(1:19)"
bode1.v19.text = "The Lord will reign forever and ever!"
bode1.v20.number_net = "(1:19)"
bode1.v20.number = "(1:20)"
bode1.v20.text = "For the horses of Pharaoh came with his chariots and his footmen into the sea, and the Lord brought back the waters of the sea on them, but the Israelites walked on dry land in the middle of the sea."
bode1.v20a.number_net = "(1:19a)"
bode1.v20a.number = "(1:20a)"
bode1.v20a.text = "For the horses of Pharaoh came with his chariots and his footmen into the sea."
bode1.v20c.number_net = "(1:19c)"
bode1.v20c.number = "(1:20c)"
bode1.v20c.text = "but the Israelites walked on dry land in the middle of the sea."

bode2.title = "Ode Two"
bode2.version = properties_en_US_net.version.designation
bode2.book_abbr = //bible_en_US_net.Deuteronomy.abbr
bode2.heading1 = "The Song of Moses"
bode2.chapverse = "(32:1-43)"
bode2.heading2 = "Another ode of Moses, the writer of the Law."
bode2.refrain = "Glory to you, our God; glory to you."
bode2.v1.number = "(2:1)"
bode2.v1.text = "Listen, O heavens, and I will speak; hear, O earth, the words of my mouth."
bode2.v2.number = "(2:2)"
bode2.v2.text = "My teaching will drop like the rain, my sayings will drip like the dew, as rain drops upon the grass, and showers upon new growth."
bode2.v3.number = "(2:3)"
bode2.v3.text = "For I will proclaim the name of the Lord; you must acknowledge the greatness of our God."
bode2.v4.number = "(2:4)"
bode2.v4.text = "As for the Rock, his work is perfect, for all his ways are just."
bode2.v5.number = "(2:5)"
bode2.v5.text = "He is a reliable God who is never unjust, he is fair and upright."
bode2.v6.number = "(2:6)"
bode2.v6.text = "His people have been unfaithful to him; they have not acted like his children – this is their sin. They are a perverse and deceitful generation. Is this how you repay the Lord,"
bode2.v7.number = "(2:7)"
bode2.v7.text = "You foolish, unwise people? Is he not your father, your Creator? He has made you and established you."
bode2.v8.number = "(2:8)"
bode2.v8.text = "Remember the ancient days; bear in mind the years of past generations."
bode2.v9.number = "(2:9)"
bode2.v9.text = "Ask your father and he will inform you, your elders, and they will tell you."
bode2.v10.number = "(2:10)"
bode2.v10.text = "When the Most High gave the nations their inheritance, when he divided up humankind, he set the boundaries of the peoples, according to the number of the heavenly assembly."
bode2.v11.number = "(2:11)"
bode2.v11.text = "For the Lord’s allotment is his people, Jacob is his special possession."
bode2.v12.number = "(2:12)"
bode2.v12.text = "The Lord found him in a desolate land, in an empty wasteland where animals howl. He continually guarded him and taught him; he continually protected him like the pupil of his eye."
bode2.v13.number = "(2:13)"
bode2.v13.text = "Like an eagle that stirs up its nest, that hovers over its young, so the Lord spread out his wings and took him, he lifted him up on his pinions."
bode2.v14.number = "(2:14)"
bode2.v14.text = "The Lord alone was guiding him, no foreign god was with him."
bode2.v15.number = "(2:15)"
bode2.v15.text = "He enabled him to travel over the high terrain of the land, and he ate of the produce of the fields."
bode2.v16.number = "(2:16)"
bode2.v16.text = "He provided honey for him from the cliffs, and olive oil from the hardest of rocks,"
bode2.v17.number = "(2:17)"
bode2.v17.text = "Butter from the herd and milk from the flock, along with the fat of lambs, rams and goats of Bashan, along with the best of the kernels of wheat; and from the juice of grapes you drank wine."
bode2.v18.number = "(2:18)"
bode2.v18.text = "But Jeshurun became fat and kicked, you got fat, thick, and stuffed! Then he deserted the God who made him, and treated the Rock who saved him with contempt."
bode2.v19.number = "(2:19)"
bode2.v19.text = "They made him jealous with other gods, they enraged him with abhorrent idols."
bode2.v20.number = "(2:20)"
bode2.v20.text = "They sacrificed to demons, not God, to gods they had not known; to new gods who had recently come along, gods your ancestors had not known about."
bode2.v21.number = "(2:21)"
bode2.v21.text = "You have forgotten the Rock who fathered you, and put out of mind the God who gave you birth."
bode2.v22.number = "(2:22)"
bode2.v22.text = "But the Lord took note and despised them because his sons and daughters enraged him."
bode2.v23.number = "(2:23)"
bode2.v23.text = "He said, “I will reject them, I will see what will happen to them; for they are a perverse generation, children who show no loyalty."
bode2.v24.number = "(2:24)"
bode2.v24.text = "They have made me jealous with false gods, enraging me with their worthless gods; so I will make them jealous with a people they do not recognize, with a nation slow to learn I will enrage them."
bode2.v25.number = "(2:25)"
bode2.v25.text = "For a fire has been kindled by my anger, and it burns to lowest Sheol; it consumes the earth and its produce, and ignites the foundations of the mountains."
bode2.v26.number = "(2:26)"
bode2.v26.text = "I will increase their disasters, I will use up my arrows on them."
bode2.v27.number = "(2:27)"
bode2.v27.text = "They will be starved by famine, eaten by plague, and bitterly stung; I will send the teeth of wild animals against them, along with the poison of creatures that crawl in the dust."
bode2.v28.number = "(2:28)"
bode2.v28.text = "The sword will make people childless outside, and terror will do so inside; they will destroy both the young man and the virgin, the infant and the gray-haired man."
bode2.v29.number = "(2:29)"
bode2.v29.text = "I said, ‘I want to cut them in pieces. I want to make people forget they ever existed."
bode2.v30.number = "(2:30)"
bode2.v30.text = "But I fear the reaction of their enemies, for their adversaries would misunderstand"
bode2.v31.number = "(2:31)"
bode2.v31.text = "And say, “Our power is great, and the Lord has not done all this!”’"
bode2.v32.number = "(2:32)"
bode2.v32.text = "They are a nation devoid of wisdom, and there is no understanding among them."
bode2.v33.number = "(2:33)"
bode2.v33.text = "I wish that they were wise and could understand this, and that they could comprehend what will happen to them.”"
bode2.v34.number = "(2:34)"
bode2.v34.text = "How can one man chase a thousand of them, and two pursue ten thousand; unless their Rock had delivered them up, and the Lord had handed them over?"
bode2.v35.number = "(2:35)"
bode2.v35.text = "For our enemies’ rock is not like our Rock, as even our enemies concede."
bode2.v36.number = "(2:36)"
bode2.v36.text = "For their vine is from the stock of Sodom, and from the fields of Gomorrah. Their grapes contain venom, their clusters of grapes are bitter."
bode2.v37.number = "(2:37)"
bode2.v37.text = "Their wine is snakes’ poison, the deadly venom of cobras."
bode2.v38.number = "(2:38)"
bode2.v38.text = "“Is this not stored up with me?” says the Lord, “Is it not sealed up in my storehouses?"
bode2.v39.number = "(2:39)"
bode2.v39.text = "I will get revenge and pay them back at the time their foot slips; for the day of their disaster is near, and the impending judgment is rushing upon them!”"
bode2.v40.number = "(2:40)"
bode2.v40.text = "The Lord will judge his people, and will change his plans concerning his servants;"
bode2.v41.number = "(2:41)"
bode2.v41.text = "When he sees that their power has disappeared, and that no one is left, whether confined or set free."
bode2.v42.number = "(2:42)"
bode2.v42.text = "He will say, “Where are their gods, the rock in whom they sought security,"
bode2.v43.number = "(2:43)"
bode2.v43.text = "Who ate the best of their sacrifices, and drank the wine of their drink offerings? Let them rise and help you; let them be your refuge!"
bode2.v44.number = "(2:44)"
bode2.v44.text = "“See now that I, indeed I, am he!” says the Lord, “and there is no other god besides me. I kill and give life, I smash and I heal, and none can resist my power."
bode2.v45.number = "(2:45)"
bode2.v45.text = "For I raise up my hand to heaven, and say, ‘As surely as I live forever,"
bode2.v46.number = "(2:46)"
bode2.v46.text = "I will sharpen my lightning-like sword, and my hand will grasp hold of the weapon of judgment; I will execute vengeance on my foes, and repay those who hate me!"
bode2.v47.number = "(2:47)"
bode2.v47.text = "I will make my arrows drunk with blood, and my sword will devour flesh – the blood of the slaughtered and captured, the chief of the enemy’s leaders!’”"
bode2.v48.number = "(2:48)"
bode2.v48.text = ""
bode2.v49.number = "(2:49)"
bode2.v49.text = "Cry out, O nations, with his people, for he will avenge his servants’ blood; he will take vengeance against his enemies, and make atonement for his land and people."

bode3.title = "Ode Three"
bode3.version = properties_en_US_net.version.designation
bode3.book_abbr = //bible_en_US_net.Samuel1.abbr
bode3.heading1 = "The Prayer of Hannah"
bode3.chapverse = "(2:1-10)"
bode3.heading2 = "The barren one honors God, giving birth in a strange manner."
bode3.refrain = "You are holy, O Lord, and my spirit praises you."
bode3.v1.number = "(3:1)"
bode3.v1.text = "My heart rejoices in the Lord; my horn is exalted high because of the Lord. I loudly denounce my enemies, for I am happy that you delivered me."
bode3.v2.number = "(3:2)"
bode3.v2.text = "No one is holy like the Lord! There is no one other than you! There is no rock like our God!"
bode3.v3.number = "(3:3)"
bode3.v3.text = "Don’t keep speaking so arrogantly, letting proud talk come out of your mouth!"
bode3.v4.number = "(3:4)"
bode3.v4.text = "For the Lord is a God who knows; he evaluates what people do."
bode3.v5.number = "(3:5)"
bode3.v5.text = "The bows of warriors are shattered, but those who stumble find their strength reinforced."
bode3.v6.number = "(3:6)"
bode3.v6.text = "Those who are well-fed hire themselves out to earn food, but the hungry no longer lack. Even the barren woman gives birth to seven, but the one with many children withers away."
bode3.v7.number = "(3:7)"
bode3.v7.text = "The Lord both kills and gives life; he brings down to the grave and raises up."
bode3.v8.number = "(3:8)"
bode3.v8.text = "The Lord impoverishes and makes wealthy; he humbles and he exalts."
bode3.v9.number = "(3:9)"
bode3.v9.text = "He lifts the weak from the dust; he raises the poor from the ash heap to seat them with princes and to bestow on them an honored position."
bode3.v10.number = "(3:10)"
bode3.v10.text = "He watches over his holy ones, but the wicked are made speechless in the darkness,"
bode3.v11.number = "(3:11)"
bode3.v11.text = "For it is not by one’s own strength that one prevails."
bode3.v11b.number = "(3:11b)"
bode3.v11b.text = ""
bode3.v12.number = "(3:12)"
bode3.v12.text = ""
bode3.v13.number = "(3:13)"
bode3.v13.text = ""
bode3.v14.number = "(3:14)"
bode3.v14.text = "The Lord shatters his adversaries; he thunders against them from the heavens. The Lord executes judgment to the ends of the earth."
bode3.v15.number = "(3:15)"
bode3.v15.text = "He will strengthen his king and exalt the power of his anointed one."

bode4.title = "Ode Four"
bode4.version = properties_en_US_net.version.designation
bode4.book_abbr = //bible_en_US_net.Habakkuk.abbr
bode4.heading1 = "The Prayer of Habakkuk"
bode4.chapverse = "(3:1-19)"
bode4.heading2 = "Habakkuk, declare the condescension of the Word."
bode4.refrain = "Glory to your power, O Lord."
bode4.v1.number = "(4:1)"
bode4.v1.text = "Lord, I have heard the report of what you did; I am awed, Lord, by what you accomplished."
bode4.v2.number = "(4:2)"
bode4.v2.text = "In our time repeat those deeds; in our time reveal them again. But when you cause turmoil, remember to show us mercy!"
bode4.v2b.number = "(4:2b)"
bode4.v2b.text = "But when you cause turmoil, remember to show us mercy!"
bode4.v3.number = "(4:3)"
bode4.v3.text = "God comes from Teman, the sovereign one from Mount Paran."
bode4.v4.number = "(4:4)"
bode4.v4.text = "His splendor covers the skies, his glory fills the earth."
bode4.v5.number = "(4:5)"
bode4.v5.text = "He is as bright as lightning; a two-pronged lightning bolt flashes from his hand. This is the outward display of his power."
bode4.v6.number = "(4:6)"
bode4.v6.text = "Plague goes before him; pestilence marches right behind him."
bode4.v7.number = "(4:7)"
bode4.v7.text = "He takes his battle position and shakes the earth; with a mere look he frightens the nations."
bode4.v8.number = "(4:8)"
bode4.v8.text = "The ancient mountains disintegrate; the primeval hills are flattened. He travels on the ancient roads."
bode4.v9.number = "(4:9)"
bode4.v9.text = "I see the tents of Cushan overwhelmed by trouble; the tent curtains of the land of Midian are shaking."
bode4.v10.number = "(4:10)"
bode4.v10.text = "Is the Lord mad at the rivers? Are you angry with the rivers? Are you enraged at the sea? Is this why you climb into your horse-drawn chariots, your victorious chariots?"
bode4.v11.number = "(4:11)"
bode4.v11.text = "Your bow is ready for action; you commission your arrows. You cause flash floods on the earth’s surface."
bode4.v12.number = "(4:12)"
bode4.v12.text = "When the mountains see you, they shake. The torrential downpour sweeps through. The great deep shouts out; it lifts its hands high."
bode4.v13.number = "(4:13)"
bode4.v13.text = "The sun and moon stand still in their courses; the flash of your arrows drives them away, the bright light of your lightning-quick spear."
bode4.v14.number = "(4:14)"
bode4.v14.text = "You furiously stomp on the earth, you angrily trample down the nations."
bode4.v15.number = "(4:15)"
bode4.v15.text = "You march out to deliver your people, to deliver your special servant. You strike the leader of the wicked nation, laying him open from the lower body to the neck."
bode4.v16.number = "(4:16)"
bode4.v16.text = "You pierce the heads of his warriors with a spear. They storm forward to scatter us; they shout with joy as if they were plundering the poor with no opposition."
bode4.v17.number = "(4:17)"
bode4.v17.text = "But you trample on the sea with your horses, on the surging, raging waters."
bode4.v18.number = "(4:18)"
bode4.v18.text = "I listened and my stomach churned; the sound made my lips quiver. My frame went limp, as if my bones were decaying, and I shook as I tried to walk."
bode4.v19.number = "(4:19)"
bode4.v19.text = "I long for the day of distress to come upon the people who attack us."
bode4.v20.number = "(4:20)"
bode4.v20.text = "When the fig tree does not bud, and there are no grapes on the vines;"
bode4.v21.number = "(4:21)"
bode4.v21.text = "when the olive trees do not produce, and the fields yield no crops;"
bode4.v22.number = "(4:22)"
bode4.v22.text = "when the sheep disappear from the pen, and there are no cattle in the stalls,"
bode4.v23.number = "(4:23)"
bode4.v23.text = "I will rejoice because of the Lord; I will be happy because of the God who delivers me!"
bode4.v24.number = "(4:24)"
bode4.v24.text = "The sovereign Lord is my source of strength. He gives me the agility of a deer;"
bode4.v25.number = "(4:25)"
bode4.v25.text = "He enables me to negotiate the rugged terrain."

bode5.title = "Ode Five"
bode5.version = properties_en_US_net.version.designation
bode5.book_abbr = //bible_en_US_net.Isaiah.abbr
bode5.heading1 = "The Prayer of Isaiah"
bode5.chapverse = "(26:9-20)"
bode5.heading2 = "The Prophecy of Isaiah"
bode5.refrain = "O Lord our God, give us peace."
bode5.v1.number = "(5:1)"
bode5.v1.text = "I look for you during the night, my spirit within me seeks you at dawn, for when your judgments come upon the earth,"
bode5.v2.number = "(5:2)"
bode5.v2.text = "Those who live in the world learn about justice."
bode5.v3.number = "(5:3)"
bode5.v3.text = "If the wicked are shown mercy, they do not learn about justice. Even in a land where right is rewarded, they act unjustly; they do not see the Lord’s majesty revealed."
bode5.v4.number = "(5:4)"
bode5.v4.text = "O Lord, you are ready to act, but they don’t even notice."
bode5.v5.number = "(5:5)"
bode5.v5.text = "They will see and be put to shame by your angry judgment against humankind, yes, fire will consume your enemies."
bode5.v6.number = "(5:6)"
bode5.v6.text = "O Lord, you make us secure, for even all we have accomplished, you have done for us."
bode5.v7.number = "(5:7)"
bode5.v7.text = "O Lord, our God, masters other than you have ruled us, but we praise your name alone."
bode5.v8.number = "(5:8)"
bode5.v8.text = "The dead do not come back to life, the spirits of the dead do not rise. That is because you came in judgment and destroyed them, you wiped out all memory of them."
bode5.v9.number = "(5:9)"
bode5.v9.text = "You have made the nation larger, O Lord, you have made the nation larger and revealed your splendor, you have extended all the borders of the land."
bode5.v10.number = "(5:10)"
bode5.v10.text = "O Lord, in distress they looked for you; they uttered incantations because of your discipline."
bode5.v11.number = "(5:11)"
bode5.v11.text = "As when a pregnant woman gets ready to deliver and strains and cries out because of her labor pains,"
bode5.v12.number = "(5:12)"
bode5.v12.text = "So were we because of you, O Lord. We were pregnant, we strained, we gave birth, as it were, to wind."
bode5.v13.number = "(5:13)"
bode5.v13.text = "We cannot produce deliverance on the earth; people to populate the world are not born."
bode5.v14.number = "(5:14)"
bode5.v14.text = "Your dead will come back to life; your corpses will rise up. Wake up and shout joyfully, you who live in the ground!"
bode5.v15.number = "(5:15)"
bode5.v15.text = "For you will grow like plants drenched with the morning dew, and the earth will bring forth its dead spirits."
bode5.v16.number = "(5:16)"
bode5.v16.text = "Go, my people! Enter your inner rooms! Close your doors behind you! Hide for a little while, until his angry judgment is over!"

bode6.title = "Ode Six"
bode6.version = properties_en_US_net.version.designation
bode6.book_abbr = //bible_en_US_net.Jonah.abbr
bode6.heading1 = "The Prayer of Jonah"
bode6.chapverse = "(2:2-9)"
bode6.heading2 = "Jonah prayed to the Lord his God from the stomach of the fish and said."
bode6.refrain = "As the Prophet Jonah, save us, O Lord."
bode6.v1.number = "(6:1)"
bode6.v1.text = "I called out to the Lord from my distress, and he answered me; from the belly of Sheol I cried out for help, and you heard my prayer."
bode6.v1a.number = "(6:1a)"
bode6.v1a.text = "I called out to the Lord from my distress, and he answered me."
bode6.v2.number = "(6:2)"
bode6.v2.text = "You threw me into the deep waters, into the middle of the sea; the ocean current engulfed me."
bode6.v3.number = "(6:3)"
bode6.v3.text = "All the mighty waves you sent swept over me."
bode6.v4.number = "(6:4)"
bode6.v4.text = "I thought I had been banished from your sight, that I would never again see your holy temple!"
bode6.v5.number = "(6:5)"
bode6.v5.text = "Water engulfed me up to my neck; the deep ocean surrounded me; seaweed was wrapped around my head. I went down to the very bottoms of the mountains; the gates of the netherworld barred me in forever."
bode6.v5a.number = "(6:5a)"
bode6.v5a.text = "Water engulfed me up to my neck; the deep ocean surrounded me."
bode6.v5b.number = "(6:5b)"
bode6.v5b.text = "Seaweed was wrapped around my head. I went down to the very bottoms of the mountains; the gates of the netherworld barred me in forever."
bode6.v6.number = "(6:6)"
bode6.v6.text = "But you brought me up from the Pit, O Lord, my God."
bode6.v7.number = "(6:7)"
bode6.v7.text = "When my life was ebbing away, I called out to the Lord, and my prayer came to your holy temple."
bode6.v7b.number = "(6:7b)"
bode6.v7b.text = "And my prayer came to your holy temple."
bode6.v8.number = "(6:8)"
bode6.v8.text = "Those who worship worthless idols forfeit the mercy that could be theirs."
bode6.v9.number = "(6:9)"
bode6.v9.text = "But as for me, I promise to offer a sacrifice to you with a public declaration of praise; I will surely do what I have promised. Salvation belongs to the Lord!"
bode6.v9b.number = "(6:9b)"
bode6.v9b.text = "Salvation belongs to the Lord!"

